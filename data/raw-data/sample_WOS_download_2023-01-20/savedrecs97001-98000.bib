@article{ WOS:000476517700006,
Author = {Heal, Katherine R. and Kellogg, Natalie A. and Carlson, Laura T. and
   Lionheart, Regina M. and Ingalls, Anitra E.},
Title = {Metabolic Consequences of Cobalamin Scarcity in the Diatom Thalassiosira
   pseudonana as Revealed Through Metabolomics},
Journal = {PROTIST},
Year = {2019},
Volume = {170},
Number = {3},
Pages = {328-348},
Month = {JUL},
Abstract = {Diatoms perform an estimated 20\% of global photosynthesis, form the
   base of the marine food web, and sequester carbon into the deep ocean
   through the biological pump. In some areas of the ocean, diatom growth
   is limited by the micronutrient cobalamin (vitamin B-12), yet the
   biochemical ramifications of cobalamin limitation are not well
   understood. In a laboratory setting, we grew the diatom Thalassiosira
   pseudonana under replete and low cobalamin conditions to elucidate
   changes in metabolite pools. Using metabolomics, we show that the diatom
   experienced a metabolic cascade under cobalamin limitation that affected
   the central methionine cycle, transsulfuration pathway, and composition
   of osmolyte pools. In T. pseudonana, 5'-methylthioadenosine decreased
   under low cobalamin conditions, suggesting a disruption in the diatom's
   polyamine biosynthesis. Furthermore, two acylcarnitines accumulated
   under low cobalamin, suggesting the limited use of an
   adenosylcobalamin-dependent enzyme, methylmalonyl CoA mutase. Overall,
   these changes in metabolite pools yield insight into the metabolic
   consequences of cobalamin limitation in diatoms and suggest that
   cobalamin availability may have consequences for microbial interactions
   that are based on metabolite production by phytoplankton. (C) 2019
   Elsevier GmbH. All rights reserved.},
