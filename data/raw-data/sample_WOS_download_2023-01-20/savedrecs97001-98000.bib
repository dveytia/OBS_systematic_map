@article{ WOS:000365056000002,
Author = {Ge, Lu and Jiang, Shao-Yong and Blumenberg, Martin and Reitner, Joachim},
Title = {Lipid biomarkers and their specific carbon isotopic compositions of cold
   seep carbonates from the South China Sea},
Journal = {MARINE AND PETROLEUM GEOLOGY},
