@article{ WOS:000698674400002,
Author = {Yang, Weifeng and Zhang, Xinxing and Chen, Min and Fang, Ziming and Qiu,
   Yusheng},
Title = {Utilizing Th-234/U-238 disequilibrium to constrain particle dynamics in
   hydrothermal plumes in the Southwest Indian Ocean},
Journal = {ACTA OCEANOLOGICA SINICA},
Year = {2021},
Volume = {40},
Number = {6},
Pages = {16-25},
Month = {JUN},
Abstract = {Metal-enriched minerals have been widely observed near hydrothermal vent
   fields. However, the dynamics of particulate metals influenced by
   hydrothermal activities is poorly constrained. Here, radioactive Th-234
   in both dissolved and particulate phases were used to examine the
   kinetics of particle-reactive metal adsorption, removal, and residence
   in a newly found hydrothermal plume over the Southwest Indian Ridge. The
   results showed a relatively low value on Th-234/U-238 ratios (i.e.,
   0.73-0.88) compared to the deep oceans, indicating an enhanced
   adsorption of particle-reactive metals onto particulate matter in the
   plume. Based on the Th-234-U-238 disequilibria, the adsorption and
   sinking rate constants of Th-234 averaged (0.009 +/- 0.001) d(-1) and
   (0.113 +/- 0.024) d(-1) in the hydrothermal plume, corresponding to the
   residence times of (115 +/- 19) d and (16 +/- 5) d for dissolved and
   particulate Th-234, respectively. This timescale allows vent-discharged
   particle-reactive metals to disperse hundreds to thousands of miles
   away. Thus, hydrothermal activities might influence the metal
   distribution in deep ocean over a very large scope. Also, a high sinking
   flux of (36.2 +/- 5.4) Bq/(m(2)center dot d) for Th-234 was observed for
   the plume, suggesting an enrichment of metal in particles deposited
   close to the vent. The enhancement of particle sinking could also
   benefit the transport of organic carbon and nitrogen and fuel the
   benthic ecosystems under the plume regimes. Thus, hydrothermal plumes
   may have an impact on both the elemental geochemistry and/or ecosystem
   to the deep oceans interior than previous expectation.},
