@inproceedings{ WOS:000089401600046,
Author = {Mastrangelo, CF and Henriques, CCD},
Editor = {Chung, JS and Frederking, RMW and Saeki, H and Koterayama, W},
Title = {Petrobras experience on the operation of FPSOs},
Booktitle = {PROCEEDINGS OF THE 10TH (2000) INTERNATIONAL OFFSHORE AND POLAR
   ENGINEERING CONFERENCE, VOL I},
Series = {International Offshore and Polar Engineering Conference Proceedings},
