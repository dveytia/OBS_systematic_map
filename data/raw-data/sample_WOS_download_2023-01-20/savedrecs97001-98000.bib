@inproceedings{ WOS:000684618700031,
Author = {Sjah, T. and Halil and Budastra, I and Tanaya, I. G. L. P.},
Book-Group-Author = {IOP},
Title = {Developing porang agribusiness for multiple stakeholder benefits and
   supporting sustainable development in dryland areas of Lombok},
Booktitle = {3RD INTERNATIONAL CONFERENCE ON BIOSCIENCE AND BIOTECHNOLOGY},
