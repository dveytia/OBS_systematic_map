@article{ WOS:000256131900003,
Author = {He Jianhua and Ma Hao and Chen Liqi and Xiang Baoqiang and Zeng
   Xianzhang and Yin Mingduan and Zeng Wenyi},
Title = {The investigation on particulate organic carbon fluxes with
