@article{ WOS:000230715500007,
Author = {Georgiou, IY and FitzGerald, DM and Stone, GW},
Title = {The impact of physical processes along the Louisiana coast},
Journal = {JOURNAL OF COASTAL RESEARCH},
Year = {2005},
Number = {44},
Pages = {72-89},
Month = {SPR},
Abstract = {The present-day coast of Louisiana is undergoing unprecedented change
   when compared With other coastal regions of the United States. Whereas
   most of its shoreline is retreating and its coastal bays expanding tit
   the expense of wetlands, the Wax Lake and Atchafalaya deltas are
   prograding and forming new delta plains. Coastal processes responsible
   for reworking the lower delta plain and modifying the coast are modest,
   including very low-wave energy and a microtidal regime. However,
   occasional hurricanes and more frequent frontal systems produce elevated
   water levels and large waves (heights >1 to 2 meters), which produce
   erosion, overwash, and barrier breaching. High subsidence rates coupled
   with eustatic sea-level rise and wave erosion are converting wetlands to
   open-water bays. Along barrier shorelines, this process is increasing
   tidal exchange, which is enlarging tidal inlets and the volume of sand
   captured in ebb-tidal deltas. Sequestration of sand in offshore ebb
   shoals depletes sand resources to the barrier chain. The segmentation,
   landward migration, and overall decrease in size of the barriers tire a
   product of relative sea-level rise and the lack of contribution of new
   sediment to the system, Restoration or the barrier,,; should be planned
   with an understanding that the Louisiana coast is evolving in a
   transgression.},
