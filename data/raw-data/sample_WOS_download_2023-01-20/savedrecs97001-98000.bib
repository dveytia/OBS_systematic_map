@article{ WOS:000403973500224,
Author = {Prasetya, Johan Danu and Annbariyanto and Supriharyono and Purwanti,
   Frida},
Title = {Diversity Based Sustainable Management for Seagrass Ecosystem: Assessing
   Distribution and Diversity of Seagrass in Marine Protected Area},
Journal = {ADVANCED SCIENCE LETTERS},
Year = {2017},
Volume = {23},
Number = {3},
Pages = {2413-2415},
