@inproceedings{ WOS:000349891400006,
Author = {Hiemstra, Erik J. and Goldstein, Robert H.},
Editor = {Agar, SM and Geiger, S},
Title = {Repeated injection of hydrothermal fluids into downdip carbonates: a
   diagenetic and stratigraphic mechanism for localization of reservoir
   porosity, Indian Basin Field, New Mexico, USA},
Booktitle = {FUNDAMENTAL CONTROLS ON FLUID FLOW IN CARBONATES: CURRENT WORKFLOWS TO
   EMERGING TECHNOLOGIES},
