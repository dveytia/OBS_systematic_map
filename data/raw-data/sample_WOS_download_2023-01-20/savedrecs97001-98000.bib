@article{ WOS:000245178000003,
Author = {Riesinger, P. and Hyvonen, T.},
Title = {Impact of management on weed species composition in organically cropped
   spring cereals},
Journal = {BIOLOGICAL AGRICULTURE \& HORTICULTURE},
Year = {2006},
Volume = {24},
Number = {3},
Pages = {257-274},
Abstract = {Partial Redundancy Analysis (pRDA) was applied to analyse the
   relationship between weed species composition and agricultural
   management measures in organically cropped spring cereal fields. Density
   and dry weight of weed species were assessed in 30 fields situated in
   the coastal regions of Finland. The pRDA analysis included nine
   explanatory variables describing management (years since conversion to
   organic farming, crop rotation, crop, soil pH, fertilization, burial of
   manure, weed management, application of stubble cultivation and crop dry
   weight) and two not manipulable variables (location of the field and
   soil type). The only statistically significant variables appeared to be
   crop dry weight, years since conversion to organic farming and cropping
   of wheat. The abundances of Elymus repens and Cirsium arvense increased
   as a function of years since conversion to organic farming. There was no
   relationship between the ratio of non-nitrophilous to nitrophilous
   species and the duration of organic farming. The abundances of C.
   arvense and Sonchus arvensis as well as of most annual species were
   inversely related to crop dry weight and they were not associated with
   cropping of wheat. The shade-tolerant species Chenopodium spp., Fallopia
   convolvulus and Viola arvensis maintained their dry weight in conditions
   of increasing crop dry weight and cropping of wheat. Galium spp. were
   associated with cropping of wheat. Weed species associated with years
   since conversion to organic farming were only in a few cases identical
   with species associated with low crop dry weight, which emphasizes the
   importance of both short- and long-term agricultural management with
   regard to weed control in organic farming.},
