@inproceedings{ WOS:000185208000129,
Author = {Kojima, R and Yamane, K and Aya, I},
Editor = {Gale, J and Kaya, Y},
Title = {Dual nature of CO2 solubility in hydrate forming region},
Booktitle = {GREENHOUSE GAS CONTROL TECHNOLOGIES, VOLS I AND II, PROCEEDINGS},
Year = {2003},
Pages = {825-830},
Note = {6th International Conference on Greenhouse Gas Control Technologies,
   KYOTO, JAPAN, OCT 01-04, 2002},
Organization = {IEA Greenhouse Gas Programme; RITE; JSER},
Abstract = {We observed that a CO2 droplet was not often covered with hydrate
   membrane until its complete dissolution, even in a hydrate-forming
   region, and the dissolution rate was higher than the case with a hydrate
   membrane. The difference of dissolution rates increases in proportion to
   the subcooling from the dissociation temperature, and the dissolution
   rate with hydrate membrane becomes a third of the case without hydrate
   at 2 Celsius. On the other hand, the measured CO2 Solubility with
   hydrate shows the same temperature dependence as the dissolution rate of
   a CO2 droplet covered with hydrate membrane, and the temperature
   dependence of the dissolution rate of a CO2 droplet without hydrate
   membrane is similar to that of the published solubility data for
   non-hydrate forming regions. It can be pointed out that there are two
   different solubilities in hydrate forming regions, that is, the
   solubility Of CO2 gas phase which can be extrapolated from the published
   data, and the solubility of CO2 hydrate of which data has already been
   obtained by the authors' experiment. The above-mentioned dual nature of
   CO2 solubility in hydrate-forming regions exerts a great influence on
   the evaluation Of CO2 ocean sequestration methods proposed so far.
   Especially in the dissolution method, in which CO2 is released as
   droplets in the ocean shallower than 2000 meters, this nature of
   solubility influences on the dissolution rate directly. Here, the
   authors attempt to clarify the dual nature of CO2 solubility in
   hydrate-forming regions, to propose the hypothesis of its mechanism and
   to discuss how to probe it.},
