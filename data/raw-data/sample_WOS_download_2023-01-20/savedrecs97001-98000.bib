@article{ WOS:000424024300002,
Author = {Tian, Miao and Kadri, Usama},
Title = {Wavemaker theories for acoustic-gravity waves over a finite depth},
Journal = {JOURNAL OF ENGINEERING MATHEMATICS},
Year = {2018},
Volume = {108},
