@article{ WOS:A1996TU79900012,
Author = {AlLihaibi, SS and AlOmran, L},
Title = {Petroleum hydrocarbons in offshore sediments from the Gulf},
Journal = {MARINE POLLUTION BULLETIN},
Year = {1996},
Volume = {32},
Number = {1},
Pages = {65-69},
Month = {JAN},
Abstract = {Petroleum hydrocarbons in offshore sediments from the central part of
   the Gulf were measured using fluorescence spectrophotometry.
   Concentrations varied between 4.0 and 56.2 mu g g(-1) wet sediment
   (expressed as Kuwait Crude Oil equivalents), with an average of 12.3 mu
   g g(-1). Highest concentrations were recorded in the north-west sector,
   with concentrations decreasing in a south-westerly direction. No
   significant correlations were observed between petroleum hydrocarbons
   and sedimentary organic carbon (r=-0.07), `mud' content (r=0.09), `sand'
   content (r=-0.08) or `gravel' content (r=-0.12), Distributions of oil
   are considered to relate more closely to prevailing current and
   localized pollutant sources in the region. Despite the substantial
   inputs of oil to the Gulf, contamination earn be considered
   comparatively low, possibly reflecting physical processes and biological
   degradation which accelerate removal of petroleum from this marine
   environment.},
