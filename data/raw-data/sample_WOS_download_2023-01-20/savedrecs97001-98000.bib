@article{ WOS:000354864300001,
Author = {Ghoddami, Hamidreza and Yazdani, Amirnaser},
Title = {A Mitigation Strategy for Temporary Overvoltages Caused by
   Grid-Connected Photovoltaic Systems},
Journal = {IEEE TRANSACTIONS ON ENERGY CONVERSION},
Year = {2015},
Volume = {30},
Number = {2},
Pages = {413-420},
Month = {JUN},
Abstract = {Widespread penetration of distributed generators (DGs) into the power
   system has aggravated the problem of temporary overvoltages (TOVs),
   which can inflict substantial damages on the DGs, equipment, and other
   customers. Through slight modifications to the power-electronic circuit
   and its control, this paper proposes a strategy for limiting the TOVs
   caused by single-stage grid-connected photovoltaic (PV) energy systems.
   This paper also proposes a reduced TOV mitigation strategy for
