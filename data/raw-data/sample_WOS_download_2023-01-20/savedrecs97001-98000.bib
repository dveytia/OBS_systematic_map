@article{ WOS:000368424000011,
Author = {Su, Ning},
Title = {CULTURAL SENSEMAKING IN OFFSHORE INFORMATION TECHNOLOGY SERVICE
   SUPPLIERS: A CULTURAL FRAME PERSPECTIVE},
