@article{ WOS:000406942200002,
Author = {Ringrose, Philip S.},
Title = {Principles of sustainability and physics as a basis for the low-carbon
   energy transition},
Journal = {PETROLEUM GEOSCIENCE},
Year = {2017},
Volume = {23},
