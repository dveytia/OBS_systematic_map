@article{ WOS:000709265200001,
Author = {Muduli, P. R. and Acharya, P. and Behera, A. T. and Pattnaik, A. K.},
Title = {Long-term variation of CO2 flux and the controlling factors in Asia's
   largest brackish water system, Chilika Lake},
Journal = {INTERNATIONAL JOURNAL OF ENVIRONMENTAL SCIENCE AND TECHNOLOGY},
Year = {2022},
Volume = {19},
Number = {11},
Pages = {11513-11522},
Month = {NOV},
Abstract = {Carbon dioxide (CO2) is one of the greenhouse gases known to be an
   influential driver of global warming. Recognizing the pressing need to
   identify sources and magnitudes of CO2 emissions worldwide, estimations
   of CO2 fluxes were made by analyzing 19 years of data (1999-2018) from
   30 sampling locations covering the entire Chilika Lake, India. The
   overall positive value of CO2 flux indicated that Chilika Lake acts as a
   source of CO2 to the atmosphere. Biological respiration is the major
   factor for the outflux of CO2 into the atmosphere. The C flux estimation
   (8.81 x 10(-6) PgC Y-1) from this long-term data set indicated Chilika
   contributes approximately 0.001\% of the global annual carbon emission
   and 0.002\% of the emission from global estuaries. The trend obtained
   through 19 years of monthly data indicated a significant annual increase
   in the partial pressure of CO2 (pCO(2)) (mu atm) and CO2 flux (mmol
   m(-2)d(-1)) which are 1.57 and 0.048, respectively. During the same
   period, a significant decrease in pH at the rate of 0.0006/year could be
   an indication of the impact of acidification on carbon flux.},
