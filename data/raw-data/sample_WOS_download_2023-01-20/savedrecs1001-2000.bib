@article{ WOS:000278678200014,
Author = {Chen, G. C. and Tam, N. F. Y. and Ye, Y.},
Title = {Summer fluxes of atmospheric greenhouse gases N2O, CH4 and CO2 from
   mangrove soil in South China},
Journal = {SCIENCE OF THE TOTAL ENVIRONMENT},
Year = {2010},
Volume = {408},
Number = {13},
Pages = {2761-2767},
Month = {JUN 1},
Abstract = {The atmospheric fluxes of N2O, CH4 and CO2 from the soil in four
   mangrove swamps in Shenzhen and Hong Kong. South China were investigated
   in the summer of 2008 The fluxes ranged from 0.14 to 23.83 mu mol m(-2)
