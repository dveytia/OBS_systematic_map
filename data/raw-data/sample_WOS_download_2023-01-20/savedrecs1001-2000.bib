@article{ WOS:000260539200006,
Author = {Silvennoinen, Hanna and Liikanen, Anu and Rintala, Jaana and
   Martikainen, Pertti J.},
Title = {Greenhouse gas fluxes from the eutrophic Temmesjoki River and its
   Estuary in the Liminganlahti Bay (the Baltic Sea)},
Journal = {BIOGEOCHEMISTRY},
