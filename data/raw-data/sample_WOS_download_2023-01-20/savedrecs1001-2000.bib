@inproceedings{ WOS:000298933100035,
Author = {Valpreda, Edi},
Editor = {Filho, WL},
Title = {Climate Change along Italian Coasts: Consciousness for Actions},
Booktitle = {ECONOMIC, SOCIAL AND POLITICAL ELEMENTS OF CLIMATE CHANGE},
