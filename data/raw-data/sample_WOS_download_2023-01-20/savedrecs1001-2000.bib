@article{ WOS:000356975700008,
Author = {Garfinkel, C. I. and Hurwitz, M. M. and Oman, L. D.},
Title = {Effect of recent sea surface temperature trends on the Arctic
   stratospheric vortex},
Journal = {JOURNAL OF GEOPHYSICAL RESEARCH-ATMOSPHERES},
