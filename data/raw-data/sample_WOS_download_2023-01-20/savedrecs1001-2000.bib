@article{ WOS:000722463000002,
Author = {Laughner, Joshua L. and Neu, Jessica L. and Schimel, David and Wennberg,
   Paul O. and Barsanti, Kelley and Bowman, Kevin W. and Chatterjee,
   Abhishek and Croes, Bart E. and Fitzmaurice, Helen L. and Henze, Daven
   K. and Kim, Jinsol and Kort, Eric A. and Liu, Zhu and Miyazaki, Kazuyuki
   and Turner, Alexander J. and Anenberg, Susan and Avise, Jeremy and Cao,
   Hansen and Crisp, David and de Gouw, Joost and Eldering, Annmarie and
