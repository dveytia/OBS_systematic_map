@inproceedings{ WOS:000165862700006,
Author = {Lean, JL},
Editor = {Ram, M},
Title = {The Sun, our variable star},
Booktitle = {FRAGEMENTS OF SCIENCE: FESTSCHRIFT FOR MENDEL SACHS},
Year = {1999},
Pages = {63-71},
Note = {Symposium on Fragments of Science, AMHERST, NY, SEP   07, 1997},
Abstract = {There are innumerable astronomical bodies in the universe, but only two
   are essential for life as we know it: our planet, the Earth, and the
   star about which it orbits, the Sun. Middle aged, at nearly five billion
   years, the Sun is a reliable, steady source of energy for the Earth. Its
   radiation heats the Earth's surface to -18 degreesC. Thus warmed, the
   Earth radiates its own energy which is trapped by atmospheric greenhouse
   gases and provides additional warming to 15 degreesC. Together with the
   Earth's greenhouse gases, the Sun sustains the mean global terrestrial
   temperature at a level supportive of life. It enables photosynthesis,
   and powers the atmospheric and oceanic circulations that profoundly
   influence living organisms.},
Publisher = {WORLD SCIENTIFIC PUBL CO PTE LTD},
