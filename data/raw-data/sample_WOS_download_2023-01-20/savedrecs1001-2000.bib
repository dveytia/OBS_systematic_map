@article{ WOS:000293715400007,
Author = {Sovacool, Benjamin K.},
Title = {Hard and soft paths for climate change adaptation},
Journal = {CLIMATE POLICY},
Year = {2011},
Volume = {11},
Number = {4},
Pages = {1177-1183},
Month = {JUL},
Abstract = {Amory Lovins' distinction between `soft' and `hard' paths of energy
   technologies is applied, mutatis mutandis, to humanity's efforts to
   adapt to climate change. It is argued that hard adaptive measures
   involve capital-intensive, large, complex, inflexible technology and
   infrastructure, whereas soft adaptive measures prioritize natural
   capital, community control, simplicity and appropriateness. The
   prevalence of these two types of adaptation pathways is illustrated
   through two case studies from the Maldives: The Safer Island Development
   Program and the Integrating Climate Change Risks Program. Policymakers
   must be aware that hard and soft adaptation measures may trade off with
   each other, and give both paths due consideration.},
