Title = {A vulnerability analysis approach for the Portuguese West Coast},
Booktitle = {RISK ANALYSIS V: SIMULATION AND HAZARD MITIGATION},
Series = {WIT Transactions on Ecology and the Environment},
Year = {2006},
Volume = {91},
