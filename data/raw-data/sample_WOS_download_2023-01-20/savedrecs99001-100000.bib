@article{ WOS:000824972200001,
Author = {Guo, Junwu and Guo, Leyang},
Title = {Research on SOX online detection system of marine diesel engine based on
   LabVIEW},
Journal = {JOURNAL OF MARINE SCIENCE AND TECHNOLOGY},
Year = {2022},
Volume = {27},
Number = {3},
Pages = {1179-1191},
Month = {SEP},
Abstract = {The International Maritime Organization and environmental protection
   agencies have formulated relevant treaties to control SOX emissions from
   ships for air pollution; however, the current technology application has
   not achieved effective online detection. To adapt to the future trend of
   development, this research develops a marine diesel SOX online detection
   system based on LabVIEW. This paper first discusses the principle of SOX
   detection, then designs the overall scheme of marine diesel engine SOX
   online detection system based on LabVIEW, and finally develops the
   hardware and software of the detection system. The detection system
   includes two parts: hardware and software. The hardware is mainly
   composed of a gas pretreatment module, a SOX sensor module, a USB CAN
   analysis module and PC and so on. Two types of software are developed:
   the USB CAN analysis module software and the upper computer software
   based on LabVIEW. With the help of USB CAN analysis module software, the
   problem that SOX sensor module cannot communicate with PC directly by
   USB interface is solved effectively. The SOX online detection system of
   marine diesel engine based on LabVIEW reduces the development cost and
   cycle, and its upper computer interface is intuitive and easy to
   operate.},
