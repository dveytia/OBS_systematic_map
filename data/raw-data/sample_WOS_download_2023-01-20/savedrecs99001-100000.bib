@inproceedings{ WOS:000325037500004,
Author = {Warren, Joshua and Das, Sujit and Zhang, Wei},
Book-Group-Author = {ASME},
Title = {MANUFACTURING PROCESS MODELING OF 100-400 kWe COMBINED HEAT AND POWER
   STATIONARY FUEL CELL SYSTEMS},
Booktitle = {PROCEEDINGS OF THE ASME 10TH FUEL CELL SCIENCE, ENGINEERING, AND
   TECHNOLOGY CONFERENCE, 2012},
