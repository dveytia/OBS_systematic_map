@article{ WOS:000539127200001,
Author = {Liu, Shang and Huang, Congliang},
Title = {A stable and flexible carbon black/polyethyleneimine-bacterial cellulose
   photothermal membrane for high-efficiency solar vapor generation},
Journal = {INTERNATIONAL JOURNAL OF ENERGY RESEARCH},
