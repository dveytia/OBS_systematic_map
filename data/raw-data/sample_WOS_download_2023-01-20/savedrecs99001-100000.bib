@article{ WOS:000323464000006,
Author = {Matetskiy, A. V. and Bondarenko, L. V. and Gruznev, D. V. and Zotov, A.
   V. and Saranin, A. A. and Chou, J. P. and Hsing, C. R. and Wei, C. M.
   and Wang, Y. L.},
Title = {Peculiar diffusion of C-60 on In-adsorbed Si(111)root 3 x root 3-Au
   surface},
Journal = {SURFACE SCIENCE},
Year = {2013},
Volume = {616},
Pages = {44-50},
Month = {OCT},
Abstract = {In-accumulated Si(111)root 3 x root 3-Au surface represents a
   highly-ordered homogeneous Au/Si(111) reconstruction with a
   two-dimensional gas of In adatoms on it. Regularities of C-60 migration
   on this surface have been elucidated through analysis of C-60 island
   density as a function of growth temperature and deposition rate in the
   framework of the rate equation theory and simulation of C-60 migration
   using density-functional-theory calculations. The critical cluster size
   has been found to be i = 1 for the whole temperature range studied, from
   110 to 240 K, while activation energy for C-60 diffusion varies from (99
   +/- 18) meV at 110 divided by 140 K to (370 +/- 24) meV at 160 divided
   by 240 K. This finding has been accounted to the peculiarity of C-60
   migration in a labyrinth built of In adatoms on the Si(111)root 3 x root
   3-Au surface, namely, at low temperatures C-60 migration is confined
   within the labyrinth channels, while at high temperatures C-60 molecules
   possess enough thermal energy to surmount the labyrinth walls. (C) 2013
   Elsevier B.V. All rights reserved.},
Publisher = {ELSEVIER},
Address = {RADARWEG 29, 1043 NX AMSTERDAM, NETHERLANDS},
Type = {Article},
Language = {English},
Affiliation = {Saranin, AA (Corresponding Author), Inst Automat \& Control Proc, 5 Radio St, Vladivostok 690041, Russia.
