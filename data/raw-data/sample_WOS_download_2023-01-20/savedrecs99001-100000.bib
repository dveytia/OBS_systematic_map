@article{ WOS:000281306800005,
Author = {Abdel-Hafez, Hanan F. and Abdel-Aziz, M. A.},
Title = {Synergistic Effects of some Plant Extracts to Biorational Product,
   Spinosad against the Cotton Leaf worm, Spodoptera littoralis (Biosd.)
   (Lepidoptera: Noctuidae)},
Journal = {EGYPTIAN JOURNAL OF BIOLOGICAL PEST CONTROL},
Year = {2010},
Volume = {20},
Number = {1},
Pages = {27-32},
Abstract = {Laboratory investigation revealed that emulsified oils of Tagetes,
   sesame and (soybean + sunflower) enhanced the activity and persistence
   of the bio-product, Spinosad 24SC against the 2(nd) instar larvae of the
   cotton leafworm, Spodoptera littoralis (Boisd.). Physico-chemical
   properties (suspensibility, viscosity and pH value) of Spinosad either
   alone or in combination with tested plant extracts (vegetable oils) were
   also studied under field rates. The obtained results clearly showed
   that, all the tested oils were compatible with Spinosad; they gave
   homorganic suspension and increased its viscosity and pH value. Tagetes
   oil scored the highest increase in both viscosity and pH value. The data
   proved that all the tested vegetable oils induced synergistic effect
   when mixed with Spinosad according to synergistic ratio (S.R.). The most
   evaluated promising synergism, occurred when Spinosad was combined with
   0.25\% Tagetes oil, followed by 0.5\% sesame oil, while 0.5\% (soybean +
   sunflower 1:1) oil with ratio of 1:1 was the lowest one. Spinosad +
   Tagetes oil were 3.5, 4.8 and 4.3 times more toxic for the target insect
   pest than Spinosad alone at 48, 72 and 168 hr post-exposure. Tagetes oil
   reduced LC50 of Spinosad from 30.91, 22.179 and 9.764 ppm to 8.73, 4.582
   and 2.269 ppm after 48, 72 and 168 hr, respectively. The synergistic
   effect may be due to the addition of vegetable oil that could increase
   the viscosity of Spinosad spray solution. This would increase the
   deposit on the treated plant leaves, reduce the drift and increase the
   persistence and improving the target pest control. Also, addition of
   vegetable oils inhibits respiration and thus, in turn may synergize the
   toxicity of Spinosad that can act on the nervous system.},
