@article{ WOS:000406818500027,
Author = {Chew, Clara and Lowe, Stephen and Parazoo, Nicholas and Esterhuizen,
   Stephan and Oveisgharan, Shadi and Podest, Erika and Zuffada, Cinzia and
   Freedman, Adam},
Title = {SMAP radar receiver measures land surface freeze/thaw state through
   capture of forward-scattered L-band signals},
Journal = {REMOTE SENSING OF ENVIRONMENT},
Year = {2017},
Volume = {198},
Pages = {333-344},
Month = {SEP 1},
Abstract = {Quantifying freeze/thaw status of the landscape (soils and vegetation)
   is important for identifying the availability of liquid water in the
   high latitudes to support ecosystem processes controlling carbon
   sequestration and release. A better understanding of the seasonal
   transition of high latitude landscapes between frozen and thawed states
   is therefore critical for predicting carbon cycle feedbacks in a warming
   climate. However, observations of freeze/ thaw state are not at
   sufficient temporal and spatial resolution to observe fast (similar to
   days-weeks) transitions in highly heterogeneous regions. Here, we
   present new evidence that reflected Global Navigation Satellite System
   (GNSS) signals captured by the SMAP radar receiver have the potential to
   quantify changes in high-latitude soil freeze/thaw state at
   kilometer-scale resolution. Boreal wetland observations indicate a
   similar to 10 dB seasonal difference in the signal-to-noise ratio (SNR),
   in agreement with simulations from a simple reflectivity model. Our
   analysis indicates that ground-reflected GNSS signals as a form of
   L-band bistatic radar could be a robust observable to quantify soil
   freeze/thaw state at process-relevant scales. We describe the caveats
   and potential limitations of this approach, and highlight areas for
   continuing research. (C) 2017 Elsevier Inc All rights reserved.},
