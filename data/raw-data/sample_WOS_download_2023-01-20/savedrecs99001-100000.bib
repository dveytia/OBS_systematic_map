@article{ WOS:000263656200007,
Author = {Romar, A. and Gago, C. and Fernandez-Marcos, M. L. and Alvarez, E.},
Title = {Influence of Fluoride Addition on the Composition of Solutions in
   Equilibrium with Acid Soils},
