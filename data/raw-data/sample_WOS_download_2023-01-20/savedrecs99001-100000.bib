@article{ WOS:000709675100004,
Author = {Grancharova, Valentina},
Title = {CHALLENGES TO THE MULTIMODAL TRANSPORT NETWORKS BASED ON SUSTAINABLE
   GROWTH IN THE VOLUME OF CONTAINERIZED CARGOES},
Journal = {PEDAGOGIKA-PEDAGOGY},
Year = {2021},
Volume = {93},
Number = {7, S},
Pages = {53-64},
Abstract = {Globalization is a significant factor for optimization of the existing
   transport chains. It is a major power for sustainable development of
   containerized transport, the relevant transport infrastructure and
   terminal equipment.
   The development of seaports and inland waterway connections became
   important part in global supply chain. The transportation models which
   are using multimodal transport service consist from maritime, port and
   hinterland parts. Choosing the appropriate strategy allows the shipping
   companies to reduce the transport cost and helps them to be more
   competitive on the cargo market. This article aims to identify the
   challenges and prospects for development of multimodal transport
