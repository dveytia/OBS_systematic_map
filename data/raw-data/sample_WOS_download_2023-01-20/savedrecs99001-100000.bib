@article{ WOS:A1991GD73800008,
Author = {BANERJEE, R and IYER, SD and DUTTA, P},
Title = {BURIED NODULES AND ASSOCIATED SEDIMENTS FROM THE CENTRAL INDIAN BASIN},
Journal = {GEO-MARINE LETTERS},
Year = {1991},
Volume = {11},
Number = {2},
Pages = {103-107},
Abstract = {Buried nodules from siliceous sediments in the central Indian Basin are
   morphologically variable and mineralogically consist of d-MnO2 and
   incipient todorokite.  Compositionally they are weakly diagenetic.  The
   sediment coarse fractions (> 63-mu-m) at different depths show variable
   abundances of micronodules, volcanic glass shards and biodebris. 
   Dissolution of biodebris increases and abundance of micronodules
   decreases with increasing depth.  Enrichment in Mn, Fe, Cu, Ni, Co,
   together with a decrease in organic carbon in the sediment column, may
   result from diagenetic metal remobilization.  Diagenetically remobilized
   trace metals might have been utilized for the growth of micronodules
   over the buried nodules.},
