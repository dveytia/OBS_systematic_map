@inproceedings{ WOS:000471926300001,
Author = {Pankratz, Thomas},
Editor = {Missimer, TM and Jones, B and Maliva, RG},
Title = {Overview of Intake Systems for Seawater Reverse Osmosis Facilities},
Booktitle = {INTAKES AND OUTFALLS FOR SEAWATER REVERSE-OSMOSIS DESALINATION
   FACILITIES: INNOVATIONS AND ENVIRONMENTAL IMPACTS},
Series = {Environmental Science and Engineering-Environmental Engineering},
Year = {2015},
Pages = {3-17},
Note = {Workshop on Desalination Intakes and Outfalls, King Abdullah Univ Sci \&
   Technol, Thuwal, SAUDI ARABIA, OCT 07-08, 2013},
Organization = {King Abdullah Univ Sci \& Technol, Water Desalinat \& Reuse Ctr; King
   Abdullah Univ Sci \& Technol, Red Sea Res Ctr},
Abstract = {The intake is a critical component of every seawater reverse osmosis
   facility and controls to a great degree the design and operational cost
   of downstream treatment processes. Two general classes of intake types
   occur; surface or open-ocean intakes and subsurface intakes. Globally,
   most large-capacity SWRO plants use open-ocean intake systems with the
   actual intake located either onshore (commonly shared with a power
   plant) or offshore. The most common offshore intake type uses a velocity
   cap at the top of the invert pipe. Inshore or offshore passive screen
   intakes are used to reduce the impacts of impingement and entrainment.
   Subsurface intake systems, either wells or galleries, are being used in
   hundreds of small to medium capacity SWRO facilities. Because of the
   greater attention being given to the environmental impacts of
