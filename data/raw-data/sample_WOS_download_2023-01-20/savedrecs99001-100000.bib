@article{ WOS:000306286200011,
Author = {Gower, J. and King, S.},
Title = {Use of satellite images of chlorophyll fluorescence to monitor the
   spring bloom in coastal waters},
Journal = {INTERNATIONAL JOURNAL OF REMOTE SENSING},
Year = {2012},
Volume = {33},
Number = {23, SI},
