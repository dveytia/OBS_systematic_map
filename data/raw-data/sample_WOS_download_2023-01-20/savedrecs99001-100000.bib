@article{ WOS:000722429200004,
Author = {Jones, Elizabeth M. and Chierici, Melissa and Menze, Sebastian and
   Fransson, Agneta and Ingvaldsen, Randi B. and Lodemel, Helene Hodal},
Title = {Ocean acidification state variability of the Atlantic Arctic Ocean
   around northern Svalbard},
Journal = {PROGRESS IN OCEANOGRAPHY},
Year = {2021},
Volume = {199},
Month = {DEC},
Abstract = {The Svalbard shelf and Atlantic Arctic Ocean are a transition zone
   between northward flowing Atlantic Water and ice-covered waters of the
   Arctic. Effects of regional ocean warming, sea ice loss and greater
   influence of Atlantic Water or ``Atlantification{''} on the state of
   ocean acidification, i.e. calcium carbonate (CaCO3) saturation (O) are
   yet to be fully understood. Anomalies in surface layer Omega for the
   climatically-vulnerable CaCO3 mineral aragonite (Delta Omega) were
   determined by considering the variability in Omega(aragonite) during
   late summer each year from 2014 to 2017 relative to the four-year
   average. Greatest sea ice extent and more Arctic-like conditions in 2014
   resulted in Delta Omega anomalies of -0.05 to-0.01 (up to 45\% of total
   Delta Omega) as a result of lower primary production. Conversely,
   greater Atlantic Water influence in 2015 supplied the ice-free surface
   layer with nitrate, which prolonged primary production to drive Delta
   Omega anomalies of 0.01 to 0.06 (up to 45\% of total Delta Omega) in
   more Atlantic like conditions. Additionally, dissolution of CaCO3
   increased carbonate ion concentrations giving Delta Omega anomalies up
   to 0.06 (up to 52\% of total Delta Omega). These processes enhanced
   surface water Omega, which ranged between 2.01 and 2.65 across the
   region. Recent sea ice retreat in 2016 and 2017 (rate of decrease in ice
   cover of similar to 4\% in 30 days) created transitional Atlantic-Arctic
   conditions, where surface water Omega varied between 1.87 and 2.29
   driven by AO anomalies of-0.10 to 0.01 due to meltwater inputs and
   influence of Arctic waters. Anomalies as low as-0.12 from reduced CaCO3
   dissolution in 2016 further supressed Omega. Wind-driven mixing in 2017
   entrained Atlantic Water with low Omega into the surface layer to drive
   large Delta Omega anomalies of-0.15 (up to 58\% of Delta Omega). Sea-ice
   meltwater provided a minor source of carbonate ions, slightly
   counteracting dilution effects. Ice-free surface waters were substantial
   sinks for atmospheric CO2, where uptake of 20.5 mmol m(-2) day(-1)
   lowered surface water Omega. ``Atlantification{''} could exacerbate or
   alleviate acidification of the Arctic Ocean, being highly dependent on
   the numerous factors examined here that are intricately linked to the
   sea ice-ocean system variability.},
