@article{ WOS:000826931200004,
Author = {Bai Hongxin and Sun Yue and Wang Dawei and Wang Weiwei and Wu Shiguo},
Title = {Geophysical Evidence for Carbonate Platform Periphery Gravity Flows in
   the Xisha Islands, South China Sea},
Journal = {JOURNAL OF OCEAN UNIVERSITY OF CHINA},
