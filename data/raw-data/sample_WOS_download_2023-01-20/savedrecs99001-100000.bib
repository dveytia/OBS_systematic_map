   Manniche, Jesper; Larsen, Karin T., Ctr Reg \& Tourism Res, DK-3730 Nexoe, Denmark.},
DOI = {10.1177/0969776412453146},
ISSN = {0969-7764},
Keywords = {Alternative food; culinary heritage; experience staging; rural
   development; symbolic knowledge},
