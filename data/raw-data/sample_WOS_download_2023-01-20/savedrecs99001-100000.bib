@article{ WOS:000697005600011,
Author = {Feng, Hui and Feng, Xi and Feng, Weibing and Zhang, Wei},
Title = {Sensitivity of tides and tidal components to sea-level-rise in the
   Radial Sand Ridges},
Journal = {REGIONAL STUDIES IN MARINE SCIENCE},
Year = {2021},
Volume = {47},
Month = {SEP},
Abstract = {Sea-level-rise (SLR) induced by global climate would by changing tidal
   dynamics in the deltaic and coastal regimes aggravate inundation risks.
   In order to have a thorough understanding of the tidal responses to SLR
   in the RSR of the South Yellow Sea, a comprehensive analysis on the
   tidal and tidal components' responses to SLR are conducted. Spatial
   variations of tides and tidal components between SLR scenarios and
   present-day scenario are examined. Strong spatial variation in tidal
   responses to SLR occurs over the RSR. Further investigation reveals it
   is the semidiurnal tide (S2) in control of the tidal range evolution
   under SLR in this area. Strong spatial variability in the primary tidal
   amplitudes' evolution is found between northern Dongsha shoal and
   southern Lansha-yang channel in the RSR. The significantly enhanced
   shallow water tidal components, M4 and MS4, lead to increase tidal
   distortion(flood-dominant) in the nearshore under SLR. In addition, the
   natural period for tidal resonance in the RSR tends to be smaller and
   the amplification due to resonance enhances. The evolution in tidal
   asymmetry and tidal resonance adjusts the timing and height of the local
   highwater levels. Therefore, conclusion in this study can be used for
   reference to future high-water level prediction in the RSR. (C) 2021
   Elsevier B.V. All rights reserved.},
