Language = {English},
Affiliation = {Rao, CP (Corresponding Author), Univ Tasmania, Dept Geol, GPO Box 252-79, Hobart, Tas 7001, Australia.
   Rao, C. Prasada, Univ Tasmania, Dept Geol, GPO Box 252-79, Hobart, Tas 7001, Australia.
   Goodwin, I. D., Univ Tasmania, Antarctic CRC, Hobart, Tas 7001, Australia.
   Gibson, J. A. E., Univ Tasmania, Inst Antarctic \& Southern Ocean Studies, Hobart, Tas 7001, Australia.},
DOI = {10.1007/BF03176591},
