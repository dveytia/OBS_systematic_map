@inproceedings{ WOS:000492841600128,
Author = {Daiyan, Aliakbar and Daiyan, Mohammad Ali},
Book-Group-Author = {IEEE},
Title = {Determine the optimal location and capacity of wind and solar resources
   in the presence of batteries with the aim of improving the quality of
   power distribution network},
Booktitle = {2019 IEEE 5TH CONFERENCE ON KNOWLEDGE BASED ENGINEERING AND INNOVATION
   (KBEI 2019)},
Year = {2019},
Pages = {731-738},
Note = {IEEE 5th Conference on Knowledge Based Engineering and Innovation
   (KBEI), Iran Univ Sci \& Technol, Tehran, IRAN, FEB 28-MAR 01, 2019},
Organization = {IEEE; IEEE Iran Sect; Intelligent Syst Sci Soc Iran; Islam World Sci
   Citat Ctr; Iran Comp \& Video Games Fdn},
Abstract = {One of the effective methods for increasing reliability and reducing
   power losses is the use of distributed generation (DG) based on
   renewable energy. Because of the cost of proper installation and low
   cost of operation, these resources are one of the means of providing
   energy in the event of a definite occurrence and preventing the creation
   of islanding mode in the network. In this paper, three solar energy
   sources, wind turbines and batteries are reviewed to improve reliability
   and reduce power losses in distribution systems. Two reliability
   indicators are included in the objective function, which are: system
   average interruption duration index (SAIDI) and system average
   interruption frequency index (SAIFI). Two objective functions are
   defined which, in both functions, is the ultimate objective of improving
   reliability and reducing the loss of power of the distribution system.
   In the first objective function, the minimization of total network costs
   was prioritized, while in the second objective function, normalizing the
   values, the minimization was done parametrically. This problem has been
   minimized using the Improved Group Search Optimization (IGSO) algorithm.
   The model is implemented on a standard 30-bus network and several
   scenarios have been defined based on the number of units placed, and the
   performance of the model and the proposed algorithm has been evaluated.},
