@article{ WOS:000220102700001,
Author = {Wang, GL and Schimel, D},
Title = {Climate change, climate modes, and climate impacts},
Journal = {ANNUAL REVIEW OF ENVIRONMENT AND RESOURCES},
Year = {2003},
Volume = {28},
Pages = {1-28},
Abstract = {Variability of the atmospheric and oceanic circulations in the earth
   system gives rise to an array of naturally occurring dynamical modes.
   Instead of being spatially independent or spatially uniform, climate
   variability in different parts of the globe is orchestrated by one or a
   combination of several climate modes, and global changes take place with
   a distinctive spatial pattern resembling that of the modes-related
   climate anomalies. Climate impact on the dynamics of terrestrial and
   marine biosphere also demonstrates clear signals for the mode effects.
   In this review, we view modes as an important attribute of climate
   variability, changes, and impact and emphasize the emerging concept that
   future climate changes may be manifest as changes in the leading modes
   of the climate system. The focus of this review is on three of the
   leading modes: the North Atlantic Oscillation, the El Nino-Southern
   Oscillation, and the Pacific Decadal Oscillation.},
