@article{ WOS:000831252700001,
Author = {Rudianto, Rudianto and Darmawan, Very and Isdianto, Andik and Bintoro,
   Gatut},
Title = {Restoration of coastal ecosystems as an approach to the integrated
   mangrove ecosystem management and mitigation and adaptation to climate
   changes in north coast of East Java},
Journal = {JOURNAL OF COASTAL CONSERVATION},
Year = {2022},
Volume = {26},
Number = {4},
Month = {AUG},
Abstract = {Climate change is very basic and appears on earth. Climate change has
   become an issue that must be faced by humans today and in the future.
   One of the impacts of climate change can be found in coastal areas.
   Tsunamis and tidal floods repeatedly occur in coastal areas. One of the
   efforts to overcome sea level rise that causes tsunamis, erosion, and
   tidal flooding is mangrove forests. This study aims to determine public
   awareness of the occurrence of tidal flooding and tsunami and to find an
   easy and inexpensive way to overcome it. This research is integrated
   using the partial least square (PLS) approach and the coastal
   vulnerability index (CVI) approach to mangrove forests. The results
   showed that the awareness and assessment of the community to carry out
   mangrove forest restoration to overcome disasters caused by climate
   change must be managed and handled with a co-management approach..},
