@inproceedings{ WOS:000466942200017,
Author = {Chien, Lien Kwei and Huang, Wei Po and Hsu, Chih Hsiang and Hsieh, Ching
   Chieh and Fang, Chen Yang},
Editor = {Rodriguez, GR and Brebbia, CA and Almorza, D},
Title = {COASTAL DISASTER RISK ASSESSMENT AND DESIGNATION OF PROTECTION ZONING IN
   TAIWAN USING GIS},
Booktitle = {COASTAL CITIES AND THEIR SUSTAINABLE FUTURE II},
Series = {WIT Transactions on the Built Environment},
Year = {2017},
Volume = {170},
Pages = {173-184},
Note = {2nd International Conference on Coastal Cities and their Sustainable
   Future, Univ Cadiz, Cadiz, SPAIN, 2017},
Organization = {Wessex Inst; Univ Las Palmas Gran Canaria; WIT Transact Built Environm;
   Int Journal Sustainable Dev \& Planning},
Abstract = {Due to the loss of coastal land by intensified global climate change,
   the threat to the lives and property of people is becoming more serious,
   and the disaster risk assessment and management of coastal areas is
   becoming an important issue. Based on the Coastal Management Act, in
   this study, the four hazard factors such as sea-level rise, subsidence,
   storm surge flooding, and coastal erosion are discussed. Further, the
   hazard index and the designating principles for coastal protection zone
   are established and designated as first-grade and second-grade coastal
   protection zones. In addition, the vulnerability index includes physical
   factors', natural environmental factors', social-economic factors', and
   resilience factors' relationships with urban development. Use of the
   Pareto ranking method can remove the weight factor to establish a
   comprehensive assessment of the index rating respectively. Then,
   combined with coastal hazards and the vulnerability index for disaster
   risk analysis of coastal areas, a risk map can be generated via
   geographic information systems. The study results can provide a
   reference for disaster adaptation and coastal management and
