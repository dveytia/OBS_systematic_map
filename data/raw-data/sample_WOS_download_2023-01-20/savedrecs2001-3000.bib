@article{ WOS:000079485000004,
Author = {Jouzel, J and Lorius, C},
Title = {Climate evolution: from the recent past to the future},
Journal = {COMPTES RENDUS DE L ACADEMIE DES SCIENCES SERIE II FASCICULE A-SCIENCES
   DE LA TERRE ET DES PLANETES},
Year = {1999},
Volume = {328},
