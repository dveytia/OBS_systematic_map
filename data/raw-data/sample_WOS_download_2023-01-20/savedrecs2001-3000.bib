@article{ WOS:000243005500008,
Author = {Russell, Joellen L. and Dixon, Keith W. and Gnanadesikan, Anand and
   Stouffer, Ronald J. and Toggweiler, J. R.},
Title = {The Southern Hemisphere westerlies in a warming world: Propping open the
   door to the deep ocean},
Journal = {JOURNAL OF CLIMATE},
Year = {2006},
Volume = {19},
Number = {24},
Pages = {6382-6390},
Month = {DEC 15},
Abstract = {A coupled climate model with poleward-intensified westerly winds
   simulates significantly higher storage of heat and anthropogenic carbon
   dioxide by the Southern Ocean in the future when compared with the
   storage in a model with initially weaker, equatorward-biased westerlies.
   This difference results from the larger outcrop area of the dense waters
   around Antarctica and more vigorous divergence, which remains robust
   even as rising atmospheric greenhouse gas levels induce warming that
   reduces the density of surface waters in the Southern Ocean. These
   results imply that the impact of warming on the stratification of the
   global ocean may be reduced by the poleward intensification of the
   westerlies, allowing the ocean to remove additional heat and
   anthropogenic carbon dioxide from the atmosphere.},
