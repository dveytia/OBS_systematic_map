@article{ WOS:000498289300009,
Author = {Gracida-Alvarez, Ulises R. and Winjobi, Olumide and Sacramento-Rivero,
   Julio C. and Shonnard, David R.},
Title = {System Analyses of High-Value Chemicals and Fuels from a Waste
   High-Density Polyethylene Refinery. Part 2: Carbon Footprint Analysis
   and Regional Electricity Effects},
