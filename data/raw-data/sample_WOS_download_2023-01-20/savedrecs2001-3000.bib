@article{ WOS:000432861500001,
Author = {Rushby, Andrew J. and Johnson, Martin and Mills, Benjamin J. W. and
   Watson, Andrew J. and Claire, Mark W.},
Title = {Long-Term Planetary Habitability and the Carbonate-Silicate Cycle},
Journal = {ASTROBIOLOGY},
