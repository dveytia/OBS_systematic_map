@article{ WOS:000334493700010,
Author = {Soja, Gerhard and Kitzler, Barbara and Soja, Anna-Maria},
Title = {Emissions of greenhouse gases from Lake Neusiedl, a shallow steppe lake
   in Eastern Austria},
