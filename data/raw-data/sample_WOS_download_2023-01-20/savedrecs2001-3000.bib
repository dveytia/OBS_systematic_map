@article{ WOS:000340438800036,
Author = {Eshel, Gidon and Shepon, Alon and Makov, Tamar and Milo, Ron},
Title = {Land, irrigation water, greenhouse gas, and reactive nitrogen burdens of
   meat, eggs, and dairy production in the United States},
