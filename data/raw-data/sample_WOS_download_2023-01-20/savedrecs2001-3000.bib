@article{ WOS:000229619300043,
Author = {Hansen, J and Nazarenko, L and Ruedy, R and Sato, M and Willis, J and
   Del Genio, A and Koch, D and Lacis, A and Lo, K and Menon, S and
   Novakov, T and Perlwitz, J and Russell, G and Schmidt, GA and Tausnev, N},
Title = {Earth's energy imbalance: Confirmation and implications},
Journal = {SCIENCE},
Year = {2005},
Volume = {308},
Number = {5727},
Pages = {1431-1435},
Month = {JUN 3},
Abstract = {Our climate model, driven mainly by increasing human-made greenhouse
   gases and aerosols, among other forcings, calculates that Earth is now
   absorbing 0.85 +/- 0.15 watts per square meter more energy from the Sun
   than it is emitting to space. This imbalance is confirmed by precise
   measurements of increasing ocean heat content over the past 10 years.
   Implications include(i) the expectation of additional global warming of
   about 0.60 degrees C without further change of atmospheric composition;
   (ii) the confirmation of the climate system's tag in responding to
   forcings, implying the need for anticipatory actions to avoid any
   specified level of climate change; and (iii) the likelihood of
   acceleration of ice sheet disintegration and sea level rise.},
Publisher = {AMER ASSOC ADVANCEMENT SCIENCE},
Address = {1200 NEW YORK AVE, NW, WASHINGTON, DC 20005 USA},
