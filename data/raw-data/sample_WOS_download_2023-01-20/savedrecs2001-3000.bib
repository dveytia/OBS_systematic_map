@article{ WOS:000308996900034,
Author = {Darwish, Mohamed and Mohtar, Rabi and Elgendy, Youssef and Chmeissani,
   Mokhtar},
Title = {Desalting seawater in Qatar by renewable energy: a feasibility study},
Journal = {DESALINATION AND WATER TREATMENT},
Year = {2012},
Volume = {47},
Number = {1-3},
Pages = {279-294},
Month = {SEP},
Abstract = {Qatar is an arid country and suffers severe lack of natural water
   resources. Groundwater is overexploited and has become seriously
   depleted with deteriorating quality. Non-conventional alternative water
   resources, mainly desalted seawater (DW) and treated wastewater are used
   and are slowly being expanded to serve potable agricultural, and
   industrial needs. Qatar's population growth, almost doubled in less than
   10 years, has increased the demand for potable water and is exerting
   pressure on the government to build additional desalting plants. The
   multi-stage flash (MSF) desalting is the predominant method that is used
   in Qatar. Its energy consumption is high-an average 270 kJ/kg thermal
   energy and 14.4 kJ/kg pumping energy. In the present paper, several
   different types of desalting sweater methods of less consumed energy
   than the MSF are reviewed. Burning fossil fuel, mainly natural gas, to
   supply the energy needed for DW units increases the emission of air
   polluting gases as well as greenhouse gases such as CO2 and NOx.
   Moreover, concentrated brine that is discharged from the MSF plants
   pollutes the marine and terrestrial environment. This brine is at higher
   temperature than the seawater temperature and is mixed with chemicals
   such as chlorine. The present paper also examines the feasibility of
   using renewable energy resources, such as solar and wind energies, to
   run the SWRO desalting plants, as well as selecting the suitably driven
   renewable energy operating the SW. To protect the environment and to
   make the DW more sustainable as a potable water source, renewable energy
   and more energy-efficient desalting methods should be used.},
