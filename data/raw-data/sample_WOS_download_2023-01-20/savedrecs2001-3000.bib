   and Qu, Zhuohua and Rodrigues, Vasco Sanchez and Pettit, Stephen and
   Harris, Irina and Zhang, Di and Lau, Yui-yip},
Title = {Risk and cost evaluation of port adaptation measures to climate change
   impacts},
Journal = {TRANSPORTATION RESEARCH PART D-TRANSPORT AND ENVIRONMENT},
Year = {2018},
Volume = {61},
Number = {B},
