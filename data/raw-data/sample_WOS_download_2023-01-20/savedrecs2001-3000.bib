@inproceedings{ WOS:000184645800139,
Author = {Morthorst, PE},
Editor = {Williams, DJ and Durie, B and McMullan, P and Paulson, C and Smith, A},
Title = {Offshore wind farms and a green certificate market.},
Booktitle = {GREENHOUSE GAS CONTROL TECHNOLOGIES},
Year = {2001},
Pages = {845-850},
Note = {5th International Conference on Greenshouse Gas Control Technologies,
   CAIRNS, AUSTRALIA, 2000},
Organization = {Int Energy Agcy, Australian Consortium; BHP; BP Amoco; CRIRO; Australian
   Greenhouse Off; Rio Tinto; Res Inst Innovat Technol Earth; Black Coal
   Utilisat Cooperat Res Ctr; Asea Brown Boveri; NOVEM},
Abstract = {Offshore turbines are getting an increasingly more important role in the
   development of wind power, particularly in the northwestern part of
   Europe. Presently two wind farms exist in Denmark and, before 2008, 750
   MW of offshore wind power has to be established with the Danish
   utilities as the main entrepreneur. In the long-term perspective,
   offshore wind turbines are expected to play an even more important role.
   According to the latest Danish energy plan, the total capacity of wind
   turbines is assumed to increase to approx. 5400 MW by 2030, including
   more than 3600 MW offshore turbines. This implies that wind-generated
   electricity will cover more than 50\% of total electricity consumption
   by 2030. Liberalizing the electricity industry and attempts to reduce
   the emissions of greenhouse gases are presently the two dominant trends
   in European energy policy. In a Danish context, it is expected that the
   latter issue will require the contribution from renewable energy
   technologies, but at present most renewables can not compete on their
   own with conventional technologies. One model of generating additional
   payments to renewable technologies is to develop a separate green market
