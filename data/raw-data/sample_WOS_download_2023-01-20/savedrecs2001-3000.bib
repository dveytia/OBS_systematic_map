@article{ WOS:000371984700004,
Author = {Brisley, Rachel and Wylde, Richard and Lamb, Rob and Cooper, Jonathan
   and Sayers, Paul and Hall, Jim},
Title = {Techniques for valuing adaptive capacity in flood risk management},
