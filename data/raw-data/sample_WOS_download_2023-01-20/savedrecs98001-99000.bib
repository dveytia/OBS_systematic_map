@article{ WOS:000698675000013,
Author = {Awan, M. Arslan Iqbal and Raza, Ali and Riaz, Mughees and Khalil, Linta
   and Bhatti, Kamran Liaquat},
Title = {Integration of sub-gradient based coordinate for multiple renewable
   generators in microgrid},
Journal = {MATERIALS TODAY-PROCEEDINGS},
Year = {2021},
