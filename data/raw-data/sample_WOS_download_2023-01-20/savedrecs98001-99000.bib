@article{ WOS:000254024800054,
Author = {Gao, Penghui and Zhang, Lixi and Zhang, Hefei},
Title = {Performance analysis of a new type desalination unit of heat pump with
   humidification and dehumidification},
Journal = {DESALINATION},
