@article{ WOS:A1991GL01000007,
Author = {LARCHER, W and WAGNER, J and NEUNER, G and MENDEZ, M and JIMENEZ, MS and
   MORALES, D},
Title = {THERMAL LIMITS OF PHOTOSYNTHETIC FUNCTION AND VIABILITY OF LEAVES OF
   PERSEA-INDICA AND PERSEA-AMERICANA},
Journal = {ACTA OECOLOGICA-INTERNATIONAL JOURNAL OF ECOLOGY},
Year = {1991},
Volume = {12},
Number = {4},
Pages = {529-541},
Abstract = {The ranges of optimal and limiting temperatures for photosynthesis and
   survival of leaves of Persea indica, and endemic Lauracea of the Canary
   islands and the Azores, and of the avocado, Persea americana cv.
   ``Fuerte{''}, were determined using a combination of CO2 porometry, in
   vivo chlorophyll fluorometry and colorimetric detection of CO2 gas
   exchange balance.
   The optimum temperature for CO2 uptake was found to be between 18 to
   23-degrees-C for both Persea species during the cool season. At 3 to
   5-degrees-C at 36-degrees-C the net photosynthesis rates were depressed
   to 50\% of full capacity. The heat limits for photosynthetic activity
   were found to be 43-degrees-C in P. indica and 45-degrees-C in P.
   americana. At low temperatures, photosynthetic functions were not
   entirely inhibited until temperatures down to -2 and -3-degrees-C.
   Irreversible damage appeared at temperatures above 48-degrees-C and at
   -6-degrees-C in both species. At low temperatures above freezing no
   necroses were seen in the experiments. However, field observations on P.
   americana revealed that the leaves are susceptible to chilling-induced
   long-lasting photoinhibition.
   The value of tree species with stable thermal requirements and narrow
   temperature amplitudes for photosynthetic activity and survival as
   indicators of changes in climate is discussed.},
