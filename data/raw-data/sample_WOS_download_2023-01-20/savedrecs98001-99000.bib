@inproceedings{ WOS:000613079200053,
Author = {Mishra, Vivek and Varde, P. V.},
Editor = {Varde, PV and Prakash, RV and Vinod, G},
Title = {Quality Assurance in Sustaining Reliability of Nuclear Reactor Safety
   Systems},
Booktitle = {RELIABILITY, SAFETY AND HAZARD ASSESSMENT FOR RISK-BASED TECHNOLOGIES},
Series = {Lecture Notes in Mechanical Engineering},
