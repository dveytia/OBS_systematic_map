@inproceedings{ WOS:000380475300534,
Author = {Meraj, Warisha and Mishra, Sukumar},
Book-Group-Author = {IEEE},
Title = {An Approach to Increase Power In Partial Shading Condition by
   Reconfiguration},
Booktitle = {2015 ANNUAL IEEE INDIA CONFERENCE (INDICON)},
Series = {Annual IEEE India Conference},
Year = {2015},
Note = {12 IEEE Int C Elect Energy Env Communications Computer Control, New
   Delhi, INDIA, DEC 17-20, 2015},
Abstract = {Photovoltaic (PV) array output power will be largely effected because of
   Partial shading Most of the time reduction of output power happens due
   to mismatch of the voltage and currents of the PV module combination
   which are considered as mismatch losses. To reduce these losses various
   reconfiguration techniques have been proposed earlier. The losses are
   directly depended on both position of the modules and shape of shadow.
   This paper proposes a pattern to increase the power in partial shading
   condition by defining the physical position of the modules using Sumdoku
   or Addoku puzzle pattern without modifying the TCT(Tide cross tide)
   based electrical connection. The Addoku arrangement distributes the
   shade of a module in row hence the generation of power is enhanced. In
