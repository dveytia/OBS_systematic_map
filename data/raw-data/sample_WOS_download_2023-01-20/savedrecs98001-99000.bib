@article{ WOS:000367759500021,
Author = {Jeffcoate, Penny and Whittaker, Trevor and Boake, Cuan and Elsaesser,
   Bjoern},
Title = {Field tests of multiple 1/10 scale tidal turbines in steady flows},
Journal = {RENEWABLE ENERGY},
Year = {2016},
Volume = {87},
Number = {1},
Pages = {240-252},
Month = {MAR},
Abstract = {Queen's University Belfast and Wave Barrier Ltd have developed a tidal
   testing platform to test hydro-kinetic turbines at medium scale.
   Multiple turbines can be pushed through still water conditions, in
   steady-state pushing tests. Experiments were conducted to evaluate the
   interactions between two identical, mono-strut, horizontal axis tidal
   turbines (HATTs) of 1.5 m diameter (13) rotor. Their relative
   performance when located individually, in-plane and in-line are
   investigated. The data shows a high consistency in the power curves at
   different flow speeds, which indicates high repeatability in this
   Reynolds range. For an individual turbine, there is no performance
   difference when the rotor is mounted either upstream or downstream of
   the supporting structure. When placed in-plane, the turbines have no
   adverse effect on one another. When spaced in-line with 2D separation,
   there is a 63\% reduction in the performance of the downstream turbine.
   At 6D downstream this performance reduction is still 59\%, indicating
   some wake recovery between 2D and 6D, though the influence from the
   upstream rotor persists to at least 6D downstream of the first device.
