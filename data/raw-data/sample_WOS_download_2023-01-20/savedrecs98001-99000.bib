@inproceedings{ WOS:000380550000254,
Author = {Dubbs, Lindsay L. and Piehler, Michael and Colby, Jonathan},
Book-Group-Author = {IEEE},
Title = {An examination of pelagic Sargassum community primary productivity and
   nutrient cycling in the face of Gulf Stream based energy},
