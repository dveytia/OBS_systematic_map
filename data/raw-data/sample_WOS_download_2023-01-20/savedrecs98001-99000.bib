@inproceedings{ WOS:000075973900015,
Author = {Wood, JD and Bosma, KF and Ramsey, JS},
Editor = {Tait, LS},
Title = {Tidal current characteristics of St. Lucie Inlet},
Booktitle = {RETHINKING THE ROLE OF STUCTURES IN SHORE PROTECTION},
Year = {1998},
Pages = {215-230},
Note = {11th Annual National Conference on Beach Preservation Technology, ST
   PETERSBURG, FL, FEB 04-06, 1998},
Abstract = {Observations of tidal currents in the St. Lucie Inlet were performed to
   map the temporal variation in the spatial structure of flow through
   several Inlet cross sections. These high-resolution measurements of
   tidal current velocities were obtained using a vessel-mounted Acoustic
   Doppler Current Profiler (ADCP) coupled with a GPS-based integrated
   navigation system. The inlet was traversed every 45 minutes through a
   complete lunar semi-diurnal tidal cycle (12.4 hours) in late August
   1997. The ADCP provided discrete vertical current profiles along each
   transect line with a vertical resolution of 20 cm (5 independent
   vertical measurements per meter bf water depth) and a horizontal
   resolution of approximately 5 meters (dependent upon vessel speed and
   averaging interval). The near-complete coverage of the Inlet cross
   lection during this survey allowed for accurate calculation of total
   volume discharges through a complete semi-diurnal tide cycle. Analysis
   of the survey data showed current energy focused in the deeper southern
   and center channels of the Inlet. The south channel was characterized as
   `flood dominant', with stronger flood currents than ebb currents, which
   tend to transport sediments into the inlet interior. The center channel
   was characterized as `ebb dominant', with stronger ebb currents than
   flood currents, tending to move sediments out of the Inlet to form an
   ebb tide shoal. The geometry of the waterway and hard structures
   appeared to modify the spatial structure of the tidal currents through
   the Inlet cross section.},
