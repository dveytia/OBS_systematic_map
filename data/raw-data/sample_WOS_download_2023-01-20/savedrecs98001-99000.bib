Title = {Importance of depositional texture in pore characterization of subsalt
   microbialite carbonates, offshore Brazil},
Booktitle = {MICROBIAL CARBONATES IN SPACE AND TIME: IMPLICATIONS FOR GLOBAL
   EXPLORATION AND PRODUCTION},
Series = {Geological Society Special Publication},
Year = {2015},
Volume = {418},
