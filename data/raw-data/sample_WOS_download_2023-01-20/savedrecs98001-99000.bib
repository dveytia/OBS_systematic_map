@article{ WOS:000417776900017,
Author = {Radtke, Thomas and Hebestreit, Helge and Puhan, Milo A. and Kriemler,
   Susi},
Title = {The 1-min sit-to-stand test in cystic fibrosis - Insights into
   cardiorespiratory responses},
Journal = {JOURNAL OF CYSTIC FIBROSIS},
