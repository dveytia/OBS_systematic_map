@article{ WOS:000312421400005,
Author = {Pellegrino, Ricardo and Martins, Tiago Leal and Pinto, Charles
   Budaszewski and Schein, Vanessa and Kucharski, Luiz Carlos and Martins
   Da Silva, Roselis Silveira},
Title = {Effect of starvation and refeeding on amino acid metabolism in muscle of
   crab Neohelice granulata previously fed protein- or carbohydrate-rich
   diets},
Journal = {COMPARATIVE BIOCHEMISTRY AND PHYSIOLOGY A-MOLECULAR \& INTEGRATIVE
   PHYSIOLOGY},
Year = {2013},
Volume = {164},
Number = {1},
Pages = {29-35},
Month = {JAN},
Abstract = {The present study assesses the effects of starvation and refeeding on
   1-{[}C-14]-methyl aminoisobutyric acid (C-14-MeAIB) uptake, C-14-total
   lipids, (CO2)-C-14 production from C-14-glycine, C-14-protein synthesis
   from C-14-leucine and Na+-K+-ATPase activity in jaw muscle of Neohelice
   granulata previously maintained on a carbohydrate-rich (HC) or
   high-protein (HP) diet. In N. granulata the metabolic adjustments during
   starvation and refeeding use different pathways according to the
   composition of the diet previously offered to the crabs. During
   starvation, (CO2)-C-14 production from C-14-glycine, and C-14-protein
   synthesis from C-14-leucine were reduced in HC-fed crabs. In crabs
   maintained on the HP or HC diet, C-14-total lipid synthesis increased
   after 15 days of starvation. In crabs fed HP diet, C-14-MeAIB uptake and
   Na+-K+-ATPase activity decreased in refeeding state. In crabs refeeding
   HC diet, C-14-MeAIB uptake and (CO2)-C-14 production decreased during
   the refeeding. In contrast, the C-14-protein synthesis increased after
   120 h of refeeding. In both dietary groups, C-14-total lipid synthesis
   increased during refeeding. Changes in the carbon amino acid flux
   between different metabolic pathways in muscle are among the strategies
   used by this crab to face starvation and refeeding. Protein or
   carbohydrate levels in the diet administered to this crab modulate the
   carbon flux between the different metabolic pathways. (C) 2012 Elsevier
   Inc. All rights reserved.},
