@inproceedings{ WOS:000252959900053,
Author = {Shibata, Masaru and Taniguchi, Keisuke and Uryu, Koji},
Editor = {Yuan, YF and Li, XD and Lai, D},
Title = {Merger of black hole and neutron star in general relativity},
Booktitle = {ASTROPHYSICS OF COMPACT OBJECTS},
Series = {AIP Conference Proceedings},
Year = {2007},
Volume = {968},
Pages = {297+},
Note = {International Conference on Astrophysics of Compact Objects, Huangshan,
   PEOPLES R CHINA, JUL 01-07, 2007},
Organization = {Natl Nat Sci Fdn China; Chinese Astron Soc; Nanjing Univ, Dept Astron;
   Natl Astron Observ China, Res Grp Compact Objects \& Diffuse Medium;
   Inst High Energy Phys, Particle Astrophys Lab; Peking Univ, Astron Dept;
   Univ Sci \& Technol China},
Abstract = {Some of the latest results of the simulation for the merger of black
   hole (1311)-neutron star (NS) binaries in full general relativity is
   presented. As the initial condition, we prepare a quasicircular state in
   which the BH is modeled by a moving puncture with no spin. The Gamma-law
   equation of state with Gamma = 2 and irrotational velocity field are
   used for modeling the NS. The BH mass is chosen to be approximate to
   4M(circle dot), whereas the rest-mass of the NS is approximate to
   1.4M(circle dot) and the radius is 13 km. The NS is tidally disrupted
   near the innermost stable orbit but more than similar to 90\% of the
   material is quickly swallowed into the BH and the resultant disk mass is
   approximate to 0.1M(circle dot). The thermal energy of the material in
   the disk increases by the shock heating occurred in the collision
   between the spiral arms. Our results indicate that the merger between a
   low-mass BH and its companion NS may form a central engine of
   short-gamma-ray bursts. Gravitational waveforms are also presented. We
   find that the amplitude of gravitational waves quickly decreases after
   the tidal disruption and the amplitude of the quasinormal mode ringing
   is small.},
