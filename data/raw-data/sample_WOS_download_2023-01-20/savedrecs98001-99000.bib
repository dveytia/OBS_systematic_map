@article{ WOS:000389503300004,
Author = {Schmidt, Th and Speckmann, M. and Flege, J. I. and Mueller-Caspary, K.
   and Heidmann, I. and Kubelka-Lange, A. and Mentes, T. O. and Nino, M. A.
   and Locatelli, A. and Rosenauer, A. and Falta, J.},
Title = {Mazes and meso-islands: Impact of Ag preadsorption on Ge growth on
   Si(111)},
Journal = {PHYSICAL REVIEW B},
Year = {2016},
Volume = {94},
Number = {23},
Month = {DEC 8},
Abstract = {The preadsorption of Ag on Si(111) drastically changes the growth of Ge.
   In a temperature range from 400 degrees C to 650 degrees C, Ag
   adsorption on Si leads to the formation of a root 3x root 3-R 30 degrees
   reconstruction that exhibits a maze-like morphology on the mesoscopic
   scale, as observed by low-energy electron diffraction (LEED) and
   low-energy electronmicroscopy. This maze morphology can be attributed to
   a surface roughening on an atomic scale, induced by the re-arrangement
   of top layer atoms during the 7x7 to root 3x root 3-R 30 degrees
   transition. The subsequent deposition of Ge results in the formation of
   a wetting layer, the evolution of which has been found to be governed by
   the Ag/Si(111)-root 3x root 3-R 30 degrees template's maze structure, as
   the latter offers a high density of heterogeneous nucleation sites. Upon
   further Ge growth, three-dimensional islands with diameters in the
   micrometer range are formed, which exhibit a large and flat (111) top
   facet. X-ray photoemission electron microscopy reveals that during Ge
   growth, Ag is segregating to the surface very efficiently.
   Grazing-incidence x-ray diffraction and transmission electron microscopy
   have been used to study the composition, strain state and defect
   structure of the Ge islands in dependence of the growth temperature. The
   strain induced by lattice mismatch is found to be largely relaxed
   (80-90\% relaxation) in the investigated growth temperature range from
   400 to 600 degrees C, which is confirmed by high-resolution LEED
   measurements. As a main relaxation mechanism, the formation of
   interfacial misfit dislocations has been identified. Interdiffusion of
   Si into the Ge islands becomes more and more pronounced for increasing
   growth temperature, whereas the formation of twinned Ge regions can
   drastically be suppressed at higher temperature.},
Publisher = {AMER PHYSICAL SOC},
Address = {ONE PHYSICS ELLIPSE, COLLEGE PK, MD 20740-3844 USA},
Type = {Article},
Language = {English},
