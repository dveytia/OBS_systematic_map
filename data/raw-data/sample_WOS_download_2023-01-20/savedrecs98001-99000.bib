@article{ WOS:000723669300048,
Author = {Hong, Sunghoon and Dodaran, Asgar Ahadpour and Kim, Taeyoon and Kim,
   Jongyeong and Huynh, Van Men and Lee, Jooyong and Kwon, Soonchul},
Title = {Variation of Irregular Waves Passing Over an Artificial Coral Reef (ACR)},
Journal = {JOURNAL OF COASTAL RESEARCH},
Year = {2021},
Number = {114},
Pages = {524-528},
Month = {FAL},
Abstract = {To investigate the variation of irregular waves due to an Artificial
   Coral Reef (ACR), two-dimensional experiments on the wave steepness,
   wave period, and relative submergence were conducted. The results for
   the wave transmission coefficient under ACR installation indicated that
   the wave steepness and period conditions are closely related to wave
   attenuation, which has similar trends for general coastal structures,
   even under irregular wave conditions. Additionally, the total spectral
   energy decreased gradually, whereas an inconsistent peak-energy
   decrement occurred with a wave steepness of 0.032, which was expected
   because of spectral energy redistribution due to ACR. The correlation
   between the relative submergence and spectral energy was investigated
   via a spectral analysis. In high frequency domain (1.25 similar to 2.25
   f/f(p)), the composition ratio of spectral energy increased with larger
   relative submergence, whereas opposite trend was observed in low
   frequency domain (0.25 similar to 0.75 f/f(p)). The main findings of
   this study can provide basic knowledge for understanding irregular wave
   variation over the ACR.},
