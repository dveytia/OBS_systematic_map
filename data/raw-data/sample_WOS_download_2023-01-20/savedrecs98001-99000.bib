@article{ WOS:000565230500001,
Author = {Zhang, Chaolin and Xu, Jiang and Wang, Enyuan and Peng, Shoujian},
Title = {Experimental Study on the Gas Flow Characteristics and Pressure Relief
   Gas Drainage Effect under Different Unloading Stress Paths},
Journal = {GEOFLUIDS},
