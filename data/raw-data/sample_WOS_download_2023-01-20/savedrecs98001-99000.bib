@article{ WOS:000357831700004,
Author = {Peryt, Tadeusz Marek and Halas, Stanislaw and Peryt, Danuta},
Title = {Carbon and oxygen isotopic composition and foraminifers of condensed
   basal Zechstein (Upper Permian) strata in western Poland: environmental
   and stratigraphic implications},
Journal = {GEOLOGICAL JOURNAL},
Year = {2015},
Volume = {50},
Number = {4},
Pages = {446-464},
Month = {JUL-AUG},
Abstract = {The basinal facies of the Lopingian Zechstein Limestone in SW Poland
   consists of thin (often less than 1m thick) limestones and/or dolomites,
   often containing the Kupferschiefer (few tens of centimetres thick) at
   their base, and local thick (up to 90m) reefal carbonates. The C-13
   curve of these basal Zechstein deposits strongly suggests that even when
   the Kupferschiefer is lacking, the thin (condensed) sequences record the
   entire interval of the Zechstein prior to the onset of evaporite
   deposition, in contrast to the thick reef sequences which lack the
   characteristic C-13 curve for the lowermost part of the Zechstein. The
   calcite samples show considerable ranges of O-18 values. If the maximum
   O-18 values are considered to be the closest to the pristine original
   ones and if O-18(water) value=0 is assumed, then the calculated range of
   palaeotemperatures for the Kupferschiefer and Zechstein Limestone
   calcite ranges from 19 to 34 degrees C. The faunal restriction, common
   dwarf foraminifers and the predominance of lagenids in the foraminiferal
   assemblage indicate continual dysaerobic conditions and possibly
   elevated salinity of seawater during deposition of thin basinal
   Zechstein Limestone deposits. The mixing of shallow and deeper waters in
   the stratified Zechstein Basin caused by upwelling could result in
   prolific carbonate precipitation in reefs located at the slope of the
   marginal carbonate platform of the Zechstein Limestone and in isolated
   reefs related to palaeohighs within the basin; however, there is no
   isotopic record of eventual upwelling. Copyright (c) 2014 John Wiley \&
   Sons, Ltd.},
