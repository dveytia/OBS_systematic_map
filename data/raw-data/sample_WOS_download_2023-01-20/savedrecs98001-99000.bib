@article{ WOS:000183482600008,
Author = {Raczyk-Stanislawiak, U and Swietlik, J and Kasprzyk, B and Nawrocki, J},
Title = {The efficiency of different oxidation methods in total organic carbon
   analysis},
Journal = {CHEMIA ANALITYCZNA},
Year = {2003},
Volume = {48},
Number = {2},
Pages = {243-254},
Abstract = {TOC analysis is required for the quality of drinking water and sewage
   assessment. The analysis is thought to be questionable, since there are
   many commercially available TOC analysers, employing different oxidation
   methodologies. The authors have made an attempt to compare the
   efficiency of three TOC analysers: TOC 10 10, LABTOC, and GO-TOC 100
   utilising different oxidation methods, such as: persulfate/100degreesC,
   persulfate/UV and high-temperature oxidation, respectively. Humic acid,
   polyacryloamide flocculants and perfluorooctanoic acid were used as
   model compounds. None of the examined apparatus led to their complete
   oxidation. The influence of pre-oxidation with ozone and chlorine
   dioxide on the efficiency of the subsequent TOC analysis was
   investigated and was found to cause better combustion of the organic
   carbon. The storage time of the samples treated with phosphoric acid did
   not influence the efficiency of TOC analysis.},
