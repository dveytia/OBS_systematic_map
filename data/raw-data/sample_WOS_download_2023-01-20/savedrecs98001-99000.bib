@inproceedings{ WOS:000679246601087,
Author = {Nahid-Al-Masood and Saha, Tapan Kumar and Ahmed, Hasin Mussayab},
Book-Group-Author = {IEEE},
Title = {Improving Frequency Response of a Low Inertia Grid via an Appropriate
   Load Shedding Scheme},
Booktitle = {2020 IEEE POWER \& ENERGY SOCIETY GENERAL MEETING (PESGM)},
Series = {IEEE Power and Energy Society General Meeting PESGM},
Year = {2020},
Note = {IEEE-Power-and-Energy-Society General Meeting (PESGM), ELECTR NETWORK,
   AUG 03-06, 2020},
Organization = {IEEE Power \& Energy Soc},
Abstract = {The aim of this paper is to explore an appropriate load shedding scheme
   for enhancing frequency response of a low inertia power system. Amongst
   Australian states, the installed wind power generation capacity is the
   highest in South Australia. As such, a number of thermal power plants
   have been withdrawn from the generation mix. In addition, most of the
   South Australian wind power plants deploy variable speed wind machines.
   Therefore, these plants do not usually participate in frequency
   regulation. Furthermore, South Australia is connected to its adjacent
   state Victoria through a high voltage AC interconnection. During
   significant availability of wind generation and high power import
   through the interconnection, a few synchronous generators are committed
   in the South Australian network. Under such a situation, the loss of
   interconnection leaves South Australia islanded. Consequently, South
   Austrian system has to depend on conventional under frequency load
   shedding to recover the frequency excursion. However, this technique may
   not be able to stop the frequency decline in a very low inertia
   operating condition. To overcome this challenge, this paper utilizes a
   load shedding scheme, which is based on the initial rate of change of
   frequency as well as the magnitude of frequency deviation. Performance
   of this load shedding mechanism is also analysed when additional
   frequency support from variable speed wind machines are enabled in a low
   inertia grid.},
