@article{ WOS:000274890200015,
Author = {Miroslaw-Grabowska, Joanna},
Title = {Evolution of palaeolake environment in Poland during the Eemian
   Interglacial based on oxygen and carbon isotope data from lacustrine
   carbonates},
Journal = {QUATERNARY INTERNATIONAL},
Year = {2009},
Volume = {207},
Pages = {145-156},
Month = {OCT 1},
Note = {International Symposium of the FBQ on Pleistoncene and Holocene
   Paleoenvironment and Recent Processes Across NE Europe, Plateliai,
   LITHUANIA, MAY, 2007},
Abstract = {The results of oxygen and carbon stable isotope investigations of eight
   Eemian lacustrine sediments from Poland are presented. The values of
   delta O-18 changed from ca. -11 to -1 parts per thousand and delta C-13
   values oscillated between -3.5 and + 7.0 parts per thousand. The
   isotopic record correlated well with pollen, cladoceran, and diatom
   data. The evolution of the palaeolakes in Poland was reconstructed. The
   palaeolakes originated during the final phase of the Wartanian (Late
   Saalian) Glaciation. The delta O-18 and delta C-13 values of that time
   reflect the input of detrital carbonates into the basins. The boundary
   between the glaciation and the interglacial period is expressed by a
   significant decrease in delta O-18 and delta C-13 values. During the
   early Eemian, a positive trend in delta O-18 values confirms the gradual
   climatic changes. The Eemian optimum is characterized by constant delta
   O-18 and delta C-13 values. During the Early Vistulian (Weichselian)
   Glaciation, the palaeolake declined. The varying delta O-18 values
   likely reflect frequent changes in water balance between precipitation
   and evaporation associated with an influence of marine circulation. The
   fluctuations of the isotopic curves in the upper parts of the
   successions (the post-optimum) were caused by a shallowing of the basin
   by infilling with sediments. The observed shifts in the isotopic Curves
   are due to the proximity of the Baltic Sea and earlier strong oceanic
   influences. (C) 2009 Elsevier Ltd and INQUA. All rights reserved.},
