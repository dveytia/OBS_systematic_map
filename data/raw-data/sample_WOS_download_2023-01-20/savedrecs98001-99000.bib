@article{ WOS:000737505200001,
Author = {Jang, Yohan and Sun, Zhuoya and Ji, Sanghyuk and Lee, Chaeeun and Jeong,
   Daeung and Choung, Seunghoon and Bae, Sungwoo},
Title = {Grid-Connected Inverter for a PV-Powered Electric Vehicle Charging
   Station to Enhance the Stability of a Microgrid},
Journal = {SUSTAINABILITY},
Year = {2021},
Volume = {13},
Number = {24},
Month = {DEC},
Abstract = {This study proposes a grid-connected inverter for photovoltaic
   (PV)-powered electric vehicle (EV) charging stations. The significant
   function of the proposed inverter is to enhance the stability of a
   microgrid. The proposed inverter can stabilize its grid voltage and
   frequency by supplying or absorbing active or reactive power to or from
   a microgrid using EVs and PV generation. Moreover, the proposed inverter
   can automatically detect an abnormal condition of the grid, such as a
   blackout, and operate in the islanding mode, which can provide
   continuous power to local loads using EV vehicle-to-grid service and PV
   generation. These inverter functions can satisfy the requirements of the
   grid codes, such as IEEE Standard 1547-2018 and UL 1741 SA. In addition,
   the proposed inverter can not only enhance the microgrid stability but
   also charge EVs in an appropriate mode according to the condition of the
   PV array and EVs. The proposed inverter was verified through
   experimental results with four scenarios in a lab-scale testbed. These
   four scenarios include grid normal conditions, grid voltage
   fluctuations, grid frequency fluctuations, and a power blackout. The
   experimental results demonstrated that the proposed inverter could
   enhance the microgrid stability against grid abnormal conditions,
   fluctuations of grid frequency and voltage, and charge EVs in an
   appropriate mode.},
