@article{ WOS:000427723900021,
Author = {Ogorodov, S. A. and Arkhipov, V. V. and Baranskaya, A. V. and Kokin, O.
   V. and Romanov, A. O.},
Title = {The Influence of Climate Change on the Intensity of Ice Gouging of the
   Bottom by Hummocky Formations},
Journal = {DOKLADY EARTH SCIENCES},
Year = {2018},
Volume = {478},
Number = {2},
Pages = {228-231},
Month = {FEB},
Abstract = {In the present work, several results of repeated sounding of bottom ice
   gouging microrelief within the area of the underwater pipeline crossing
   of the Baydaratskaya Bay, Kara Sea, are presented. Based on the results
   of the monitoring, as well as the analysis of literature sources and
