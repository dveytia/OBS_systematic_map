@inproceedings{ WOS:000400643800096,
Author = {Chekired, Fathia and Smara, Zoubeyr and Mahrane, Achour and Chikh,
   Madjid and Berkane, Smail},
Editor = {Howlett, R and Capozzoli, A and Serra, V},
Title = {An energy flow management algorithm for a photovoltaic solar home},
Booktitle = {8TH INTERNATIONAL CONFERENCE ON SUSTAINABILITY IN ENERGY AND BUILDINGS,
   SEB-16},
Series = {Energy Procedia},
Year = {2017},
Volume = {111},
Pages = {944-953},
Note = {8th International Conference on Sustainability in Energy and Buildings
   (SEB), Politecnico Torino, Turin, ITALY, SEP 11-13, 2016},
Abstract = {In this paper, an energy flow management algorithm for a
   grid-connected-photovoltaic system with battery storage devoted to
   supply a home is presented. This algorithm favours the fulfilment of the
   home energy demand by the energy produced by the photovoltaic generator
   or stored in the batteries than that got from the grid. This algorithm
   manages the flow of energy in the house through the combination of four
   switches. It was applied to the case of a home installed in a coastal
   region of Bou-Ismail (Algeria). The efficiency of the algorithm was
   tested for two weeks, a favourable week in summer and an unfavourable
   week in winter regarding the weather conditions. The simulation of the
   on grid PV system was done using real data of the irradiation and the
   temperature acquired by the meteorological station of the Bou-Ismail
   site and a home load profile for each season.
   The results obtained revealed that the energy demand satisfaction for
   the house is high in the favourable summer week and is only 33\% in the
   winter unfavourable week. To meet a high rate of self-consumption a Home
