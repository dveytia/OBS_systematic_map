@article{ WOS:000322236000019,
Author = {Young, J. N. and Bruggeman, J. and Rickaby, R. E. M. and Erez, J. and
   Conte, M.},
Title = {Evidence for changes in carbon isotopic fractionation by phytoplankton
   between 1960 and 2010},
Journal = {GLOBAL BIOGEOCHEMICAL CYCLES},
Year = {2013},
Volume = {27},
Number = {2},
Pages = {505-515},
Month = {APR},
Abstract = {Rising CO2 is expected to drive a myriad of environmental changes in the
   surface ocean. Deciphering the phytoplankton response to this complex
   change is difficult. Here we determine whether a trend in the biological
   fractionation of stable carbon isotopes (epsilon(p)) has occurred over
   the past 50 years. epsilon(p) is primarily controlled by the acquisition
   and intracellular transport of inorganic carbon and the rate of carbon
   fixation. In turn, these processes are sensitive to phytoplankton
   physiology, community composition, and notably inorganic carbon
   availability. epsilon(p) may therefore carry a signal of biological
   response to climate change. Temporal and spatial records of epsilon(p)
   can be deciphered from the difference between the stable carbon isotopic
   composition of particulate organic matter (delta C-13(POC)) and that of
   the ambient inorganic carbon pool (delta C-13(CO2)). Here we establish a
   global record of epsilon(p) extending from the 1960s to today, extracted
   from a newly compiled data set of global measured delta C-13(POC) and
   part measured/part climatology delta C-13(CO2). We find that epsilon(p)
   has changed significantly since the 1960s in the low- to mid-latitude
   surface ocean. The increase is most pronounced in the subtropics, where
   it is estimated at > 0.015 parts per thousand per year. Our findings of
   such rates of change are further supported by a high resolution temporal
   record from a single sediment trap near Bermuda. Our results are
   consistent with the idea that epsilon(p) is affected by increased
   inorganic carbon availability driven by the rise in atmospheric CO2.},
