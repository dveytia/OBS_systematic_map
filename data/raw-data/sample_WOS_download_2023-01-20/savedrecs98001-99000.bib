@article{ WOS:000732998700015,
Author = {Wang, Xuanyi and Zhai, Xuejun and Yu, Qingping and Liu, Xiaobin and
   Meng, Xiangmin and Wang, Xinping and Wang, Lei},
Title = {Strategies of designing electrocatalysts for seawater splitting},
Journal = {JOURNAL OF SOLID STATE CHEMISTRY},
Year = {2022},
Volume = {306},
Month = {FEB},
Abstract = {Producing hydrogen by water electrolysis is a promising technology in
   the field of energy conversion. Considering the low reserves of the pure
   water, hydrogen from seawater electrolysis has more value in practical
   application. At present, the development of high-efficient and stable
   hydrogen evolution reaction (HER) and oxygen evolution reaction (OER)
   electrocatalysts is the top priority for seawater splitting. Herein, the
   electrochemical reactions mechanism involved in seawater splitting are
   systematically summarized. In addition, we sum up the current strategies
   for preparing highly active and stable electrocatalysts for seawater
   splitting from the aspects of morphological and structural engineering,
   heterostructure component modulation, and heteroatom doping. Besides the
   above discussed content, perspective and challenges of designing
   electrocatalysts for practical hydrogen production in seawater splitting
   are also pointed out.},
