@article{ WOS:000350764700017,
Author = {Tanaka, Tsuyoshi and Maeda, Yoshiaki and Veluchamy, Alaguraj and Tanaka,
   Michihiro and Abida, Heni and Marechal, Eric and Bowler, Chris and Muto,
   Masaki and Sunaga, Yoshihiko and Tanaka, Masayoshi and Yoshino, Tomoko
   and Taniguchi, Takeaki and Fukuda, Yorikane and Nemoto, Michiko and
   Matsumoto, Mitsufumi and Wong, Pui Shan and Aburatani, Sachiyo and
   Fujibuchi, Wataru},
Title = {Oil Accumulation by the Oleaginous Diatom Fistulifera solaris as
   Revealed by the Genome and Transcriptome},
Journal = {PLANT CELL},
Year = {2015},
Volume = {27},
Number = {1},
Pages = {162-176},
Month = {JAN},
Abstract = {Oleaginous photosynthetic organisms such as microalgae are promising
   sources for biofuel production through the generation of carbon-neutral
   sustainable energy. However, the metabolic mechanisms driving high-rate
   lipid production in these oleaginous organisms remain unclear, thus
   impeding efforts to improve productivity through genetic modifications.
   We analyzed the genome and transcriptome of the oleaginous diatom
   Fistulifera solaris JPCC DA0580. Next-generation sequencing technology
   provided evidence of an allodiploid genome structure, suggesting
   unorthodox molecular evolutionary and genetic regulatory systems for
   reinforcing metabolic efficiencies. Although major metabolic pathways
   were shared with nonoleaginous diatoms, transcriptome analysis revealed
   unique expression patterns, such as concomitant upregulation of fatty
   acid/triacylglycerol biosynthesis and fatty acid degradation
   (beta-oxidation) in concert with ATP production. This peculiar pattern
   of gene expression may account for the simultaneous growth and oil
   accumulation phenotype and may inspire novel biofuel production
   technology based on this oleaginous microalga.},
