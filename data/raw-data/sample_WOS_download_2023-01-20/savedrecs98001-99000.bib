@inproceedings{ WOS:000309461400163,
Author = {Jang, Yirang and Jeong, Jaehoon and Roh, Eul and Bahn, Chung-il and
   Kwon, Sanghoon and Lee, Chang Yoon and Ryu, In-Chang},
