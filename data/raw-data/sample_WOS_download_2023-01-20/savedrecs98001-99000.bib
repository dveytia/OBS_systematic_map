Language = {English},
Affiliation = {Moghiman, M (Corresponding Author), Ferdowsi Univ Mashhad, Dept Mech Engn, Mashhad, Iran.
   Zamani, Hosein; Moghiman, Mohammad; Kianifar, All, Ferdowsi Univ Mashhad, Dept Mech Engn, Mashhad, Iran.},
DOI = {10.1016/j.renene.2015.03.064},
ISSN = {0960-1481},
Keywords = {Solar energy; Solar cooker; Double-exposure solar cooker; Reflective
   surface; Response surface method},
Keywords-Plus = {CONCENTRATOR; PERFORMANCE; DESIGN},
