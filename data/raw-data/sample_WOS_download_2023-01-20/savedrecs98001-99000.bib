Author = {Aksu, Abdullah and Cetintasoglu, Mehmet Emre and Taskin, Omer Suat and
   Caglar-Balkis, Nuray},
Title = {Development of Low-Cost Silica-Based Core Material for Fire-Resistant
   Doors of Vessels},
