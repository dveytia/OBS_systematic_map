@article{ WOS:000429746100009,
Author = {Ti, Zilong and Wei, Kai and Qin, Shunquan and Li, Yongle and Mei, Dapeng},
Title = {Numerical simulation of wave conditions in nearshore island area for
   sea-crossing bridge using spectral wave model},
Journal = {ADVANCES IN STRUCTURAL ENGINEERING},
