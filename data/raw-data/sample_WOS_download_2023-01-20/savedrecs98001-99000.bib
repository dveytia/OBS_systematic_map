Title = {Identifying the stiffness reduction in the supporting tower of wind
   turbines - a numerical approach},
Booktitle = {NONDESTRUCTIVE CHARACTERIZATION AND MONITORING OF ADVANCED MATERIALS,
   AEROSPACE, CIVIL INFRASTRUCTURE, AND TRANSPORTATION XIII},
Series = {Proceedings of SPIE},
Year = {2019},
