Title = {Mainstreaming adaptation strategy for flood risk due to climate change
   impact on Jakabaring, Palembang, Indonesia},
Booktitle = {4TH INTERNATIONAL CONFERENCE ON CLIMATE CHANGE 2019 (4TH ICCC 2019)},
