@article{ WOS:000216864000002,
Author = {Little, L. Richard and Hobday, Alistair J. and Parslow, John and Davies,
   Campbell R. and Grafton, R. Quentin},
Title = {Funding climate adaptation strategies with climate derivatives},
Journal = {CLIMATE RISK MANAGEMENT},
Year = {2015},
Volume = {8},
Pages = {9-15},
Abstract = {Climate adaptation requires large capital investments that could be
   provided not only by traditional sources like governments and banks, but
   also by derivatives markets. Such markets would allow two parties with
   different tolerances and expectations about climate risks to transact
   for their mutual benefit and, in so doing, finance climate adaptation.
   Here we calculate the price of a derivative called a European put
   option, based on future sea surface temperature (SST) in Tasmania,
   Australia, with an 18 degrees C strike threshold. This price represents
   a quantifiable indicator of climate risk, and forms the basis for
   aquaculture industries exposed to the risk of higher SST to finance
   adaptation strategies through the sale of derivative contracts. Such
   contracts provide a real incentive to parties with different climate
   outlooks, or risk exposure to take a market assessment of climate
   change. Crown Copyright (C) 2015 Published by Elsevier B.V.},
