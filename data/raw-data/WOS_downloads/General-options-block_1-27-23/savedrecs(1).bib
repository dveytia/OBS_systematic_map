@article{ WOS:000480312900003,
Author = {Yang, Jeasurk and Owusu, Victor and Andriesse, Edo and Ablo, Austin
   Dziwornu},
Title = {In-Situ Adaptation and Coastal Vulnerabilities in Ghana and Tanzania},
Journal = {JOURNAL OF ENVIRONMENT \& DEVELOPMENT},
Year = {2019},
Volume = {28},
Number = {3},
Pages = {282-308},
Month = {SEP},
Abstract = {Coastal fisheries communities in sub-Saharan Africa are under high
   socioeconomic vulnerability in the face of environmental pressures. This
   article contributes to the current adaptation debate by revisiting the
   benefits of in-situ adaptation. We assess possible in-situ adaptation
   strategies amid ongoing vulnerabilities by comparing Ghana and Tanzania.
   A total of 441 household surveys were conducted in four study sites. The
   major findings of the study are as follows: First, the three major
   in-situ adaptation strategies are regular changes of nondestructive
   fisheries techniques, alternative occupations, and collective action.
   Second, all three strategies have a significant relationship with income
   change. Finally, the communities in Tanzania utilized all three
   strategies more and performed better economically than those in Ghana.
   On the basis of these insights, we suggest implications of in-situ
   adaptation for future coastal development in sub-Saharan Africa.},
Publisher = {SAGE PUBLICATIONS INC},
Address = {2455 TELLER RD, THOUSAND OAKS, CA 91320 USA},
Type = {Article},
