@inproceedings{ WOS:000478560800043,
Author = {Putra, G. A. Yoga and Koestoer, R. H. and Lestari, I.},
Book-Group-Author = {IOP},
Title = {Local resilience towards overcoming floods of local climate change for
   adaptation: A study of marunda community in north jakarta},
Booktitle = {12TH INTERNATIONAL INTERDISCIPLINARY STUDIES SEMINAR: ENVIRONMENTAL
   CONSERVATION AND EDUCATION FOR SUSTAINABLE DEVELOPMENT},
Series = {IOP Conference Series-Earth and Environmental Science},
Year = {2019},
