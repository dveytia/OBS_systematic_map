@article{ WOS:000623726700002,
Author = {Islam, Md. Mahfuzul and Amir, A. Aldrie and Begum, Rawshan Ara},
Title = {Community awareness towards coastal hazard and adaptation strategies in
   Pahang coast of Malaysia},
