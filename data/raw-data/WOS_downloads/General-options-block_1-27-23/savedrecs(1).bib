@article{ WOS:000596704700016,
Author = {Boyer, Christopher J. and Bowen, Kathryn and Murray, Virginia and
   Hadley, Johnny and Hilly, Jimmy Jaghoro and Hess, Jeremy J. and Ebi,
   Kristie L.},
Title = {Using Implementation Science For Health Adaptation: Opportunities For
   Pacific Island Countries},
Journal = {HEALTH AFFAIRS},
Year = {2020},
Volume = {39},
Number = {12},
Pages = {2160-2167},
Month = {DEC},
Abstract = {The health risks of a changing climate are immediate and multifaceted.
   Policies, plans, and programs to reduce climate-related health impacts
   exist, but multiple barriers hinder the uptake of these strategies, and
   information remains limited on the factors affecting implementation.
   Implementation science-a discipline focused on systematically examining
   the gap between knowledge and action-can address questions related to
   implementation and help the health sector scale up successful adaptation
   measures in response to climate change. Implementation science, in the
   context of a changing climate, can guide decision makers in introducing
   and prioritizing potential health adaptation and disaster risk
   management solutions, advancing sustainability initiatives, and
   evaluating and improving intervention strategies. In this article we
   highlight examples from Pacific Island countries and outline approaches
   based on implementation science to enhance the capacity of health
   systems to anticipate, prepare for, respond to, and recover from
   climate-related exposures.},
Publisher = {PROJECT HOPE},
Address = {7500 OLD GEORGETOWN RD, STE 600, BETHESDA, MD 20814-6133 USA},
Type = {Article},
Language = {English},
Affiliation = {Boyer, CJ (Corresponding Author), Univ Washington, Dept Environm \& Occupat Hlth Sci, Seattle, WA 98195 USA.
   Boyer, Christopher J.; Hess, Jeremy J., Univ Washington, Dept Environm \& Occupat Hlth Sci, Seattle, WA 98195 USA.
   Bowen, Kathryn, Inst Adv Sustainabil Studies eV, Potsdam, Germany.
