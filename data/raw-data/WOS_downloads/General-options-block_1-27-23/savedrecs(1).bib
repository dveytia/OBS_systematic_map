@article{ WOS:000441152200041,
Author = {Bir, Md. Shahidul Haque and Haider, M. Raisul and Haque, M. Mahbubul and
   Islam, Md. Shamimul and Taek-Keun, O. H. and Park, Kee Woong},
Title = {Impact and adaptation strategies of climate change in agricultural
   extension system in Bangladesh},
