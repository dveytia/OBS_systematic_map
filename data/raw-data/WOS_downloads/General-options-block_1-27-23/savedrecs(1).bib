@article{ WOS:000352296000004,
Author = {Joakim, Erin P. and Mortsch, Linda and Oulahen, Greg},
Title = {Using vulnerability and resilience concepts to advance climate change
   adaptation},
Journal = {ENVIRONMENTAL HAZARDS-HUMAN AND POLICY DIMENSIONS},
Year = {2015},
Volume = {14},
Number = {2},
Pages = {137-155},
Month = {APR 3},
Abstract = {Adaptation is necessary if we are to minimize risks associated with
   climate change impacts. Vulnerability and resilience are two important
   concepts in the literature on hazards and climate change but have been
   used in a variety of ways to investigate human interaction with a
   hazardous environment. The result is widespread adoption of the terms
   but confusion about their relationship and how best they can advance
   work on climate change adaptation. This paper critically reviews the
   different understandings of the concepts and how they relate, and then
   proposes a framework that integrates vulnerability and resilience in
   order to advance adaptation thinking, planning and implementation. The
   paper concludes with a description of how the framework will apply
   findings on unequal social vulnerability to inform adaptation options
   that increase resilience in coastal cities.},
