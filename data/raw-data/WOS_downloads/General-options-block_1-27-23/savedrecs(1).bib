@inproceedings{ WOS:000851382700025,
Author = {Kotova, Lola and Jacob, Daniela and Leissner, Johanna and Mathis, Moritz
   and Mikolajewicz, Uwe},
Editor = {Moropoulou, A and Korres, M and Georgopoulos, A and Spyrakos, C and Mouzakis, C},
Title = {Climate Information for the Preservation of Cultural Heritage: Needs and
   Challenges},
Booktitle = {TRANSDISCIPLINARY MULTISPECTRAL MODELING AND COOPERATION FOR THE
   PRESERVATION OF CULTURAL HERITAGE, PT I},
Series = {Communications in Computer and Information Science},
Year = {2019},
Volume = {961},
Pages = {353-359},
Note = {1st International Conference on Transdisciplinary Multispectral Modeling
   and Cooperation for the Preservation of Cultural Heritage (TMM\_CH),
   Eugenides Fdn, Athens, GREECE, OCT 10-13, 2018},
Organization = {Aegean Airlines; DALKAFOUKIOIKOS; Sintecno; Post Scriptum; NEOTEK;
   NeoMech; SGT SYMEON G TSOMOKOS S A},
Abstract = {Continued preservation of cultural heritage requires reliable climate
   information as input for an accurate projection of possible impacts of
   climate change. Future climate-induced outdoor risks for cultural
   heritage can in general be estimated from the information provided by
   Earth System Models (ESMs). In this paper we present the results of the
   project Climate for Culture. The project focused on damage risk
   assessment, economic impact and mitigation strategies for sustainable
   preservation of cultural heritage in times of anthropogenic climate
   change. We utilized advanced climate modelling techniques with dynamical
