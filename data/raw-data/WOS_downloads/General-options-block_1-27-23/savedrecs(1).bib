@article{ WOS:000755970900010,
Author = {Owusu-Daaku, Kwame N. and Rosko, Helen},
Title = {The discursive construction of adaptation subjects via the Ada Sea
   Defense System in the Volta River Delta of Ghana},
Journal = {ENVIRONMENT AND PLANNING E-NATURE AND SPACE},
