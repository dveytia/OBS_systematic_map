@inproceedings{ WOS:000354881700017,
Author = {Kinzig, Ann P.},
Editor = {Knapp, RH and Levi, BG and Kammen, DM},
Title = {Climate Adaptation},
Booktitle = {PHYSICS OF SUSTAINABLE ENERGY III (PSE III): USING ENERGY EFFICIENTLY
   AND PRODUCING IT RENEWABLY},
Series = {AIP Conference Proceedings},
