@article{ WOS:000454467200041,
Author = {Lomboy, Cristopher G. and Belinario, Flora and Pomeroy, Robert and
   Pedrajas, Joey and Sanchez Tirona, Raquel and Box, Steve and Roberto
   Domondon, Paolo and Balbido-Ramirez, Kristine},
Title = {Building household economic resilience to secure a future for near shore
   fishers in the Philippines},
Journal = {MARINE POLICY},
Year = {2019},
Volume = {99},
Pages = {334-342},
Month = {JAN},
Abstract = {Resilience has become a key concept for addressing the vulnerability of
   small-scale fishing households in developing countries. While effort has
   gone into defining the concept of resilience in relation to fishing
   households; very little application of the concept exists in practice.
   An economic resiliency strategy was developed that builds resilience
   through improved household assets to reduce risks and vulnerabilities. A
   foundational conclusion of the strategy is the importance of linking
   household livelihood interventions to sustainable fishing behaviors. The
   conservation enterprise approach facilitated a mutually beneficial
   relationship between biodiversity conservation and livelihoods.},
