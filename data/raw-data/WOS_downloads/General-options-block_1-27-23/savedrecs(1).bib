   Craig, Robin Kundis, Univ Utah, SJ Quinney Coll Law, 383 South Univ St, Salt Lake City, UT 84112 USA.},
DOI = {10.1007/s10584-018-2203-5},
ISSN = {0165-0009},
EISSN = {1573-1480},
Keywords = {Coastal adaptation; Coastal retreat; Insurance; Perverse incentives;
