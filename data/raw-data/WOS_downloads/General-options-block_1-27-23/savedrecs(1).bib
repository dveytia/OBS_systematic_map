@inproceedings{ WOS:000392270100150,
Author = {English, Elizabeth and Klink, Natasha and Turner, Scott},
Editor = {Lang, M and Klijn, F and Samuels, P},
Title = {Thriving with water: Developments in amphibious architecture in North
   America},
Booktitle = {3RD EUROPEAN CONFERENCE ON FLOOD RISK MANAGEMENT (FLOODRISK 2016)},
Series = {E3S Web of Conferences},
