   Hueffer, Karsten; Ehrlander, Mary; Etz, Kathy; Reynolds, Arleigh, Univ Alaska Fairbanks, Dept Vet Med, Fairbanks, AK 99775 USA.},
DOI = {10.1080/22423982.2019.1607502},
Article-Number = {1607502},
ISSN = {1239-9736},
