@article{ WOS:000385008900016,
Author = {Menon, Muktha and Ghosh, Shubhadeep and Kumar, M. Satish and Rao, M. V.
   Hanumantha and Mahesh, V. Uma and Zacharia, P. U.},
Title = {Fishermen's perception of climate change - a study from Andhra Pradesh},
Journal = {INDIAN JOURNAL OF FISHERIES},
Year = {2016},
Volume = {63},
Number = {3},
Pages = {110-119},
Abstract = {Perceptions of fishermen on climate change were studied from two coastal
   districts of Andhra Pradesh during 2011. Fishermen were interviewed to
   know their perception on climate change over the last 20 years, impact
   of change in climatic parameters on their lives as well as on marine
   fisheries and on the adaptation measures required. All fishermen
   contacted believed that climate had changed in the last two decades.
   Wind was ranked as the parameter that had changed the most in the last
   two decades. Sea status was ranked as the most problematic to fishermen.
   Avenue for safe exit from villages and coastal protection structures in
   case of natural calamities were the highest scoring adaptation measures.
   Wind was found the most critical parameter affecting marine fishery and
   overfishing was identified as the biggest problem facing fisheries.},
