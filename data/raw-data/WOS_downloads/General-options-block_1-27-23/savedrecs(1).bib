@article{ WOS:000750983200023,
Author = {Min, Byeng R. and Parker, David and Brauer, David and Waldrip, Heidi and
   Lockard, Catherine and Hales, Kristin and Akbay, Alexia and Augyte,
   Simona},
Title = {The role of seaweed as a potential dietary supplementation for enteric
   methane mitigation in ruminants: Challenges and opportunities},
