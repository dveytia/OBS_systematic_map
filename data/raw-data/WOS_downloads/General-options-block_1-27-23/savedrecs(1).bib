@article{ WOS:000268681600001,
Author = {Arnbjerg-Nielsen, K. and Fleischer, H. S.},
Title = {Feasible adaptation strategies for increased risk of flooding in cities
   due to climate change},
Journal = {WATER SCIENCE AND TECHNOLOGY},
Year = {2009},
Volume = {60},
Number = {2},
Pages = {273-281},
Abstract = {Northern Europe is one of the regions where more frequent and more
   severe storms and storm surges are expected due to climatic changes. In
   order to maintain an acceptable risk of flooding suitable adaptation
   strategies must be defined and implemented. Optimum solutions demand
   collaboration of different professionals and thus simple graphical means
   must be employed to illustrate the economic impacts of the change in
   risk of flooding. A case study indicates that urban drainage
   infrastructure capacity should be upgraded while there is currently no
   economic incentive to improve protection against sea surges.},
