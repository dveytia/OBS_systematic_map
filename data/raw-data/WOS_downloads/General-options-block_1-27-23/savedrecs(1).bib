@article{ WOS:000510301300003,
Author = {Ganase, Sherry-Ann S. and Sookram, Sandra},
Title = {Climate change knowledge at the grass roots: the case of Bequia, St.
   Vincent and the Grenadines},
Journal = {ENVIRONMENT DEVELOPMENT AND SUSTAINABILITY},
Year = {2021},
Volume = {23},
Number = {2},
Pages = {1105-1132},
Month = {FEB},
Abstract = {This study examines perception of households to climate change and
   factors that influence the selection of adaptation measures. Using
   survey data, the results suggest that households are knowledgeable and
   concerned about climate change but lack knowledge about the measures
   needed to adapt. The results also suggest that the risk and importance
   placed on climate change, vulnerability experienced with household
   flooding, drainage and expected threat of future sea level are the main
   factors that influence the adaptation decision. The results obtained in
   this study are beneficial to all as adaptation requires a collective
   effort by stakeholders.},
