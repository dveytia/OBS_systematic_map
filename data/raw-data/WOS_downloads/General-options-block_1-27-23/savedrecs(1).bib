@inproceedings{ WOS:000361211507036,
Author = {Bielicki, Jeffrey M. and Middleton, Richard S. and Levine, Jonathan S.
   and Stauffer, Phil},
Editor = {Dixon, T and Herzog, H and Twinning, S},
Title = {An Alternative Pathway for Stimulating Regional Deployment of Carbon
   Dioxide Capture and Storage},
