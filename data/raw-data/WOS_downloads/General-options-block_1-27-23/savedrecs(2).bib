@article{ WOS:000298343100010,
Author = {Yang, Chun-Yao and Wang, Hsiao-Wen and Kuo, Pin-Han},
Title = {APPLICATION OF LANDSCAPE ECOLOGICAL DECISION AND EVALUATION SUPPORT
