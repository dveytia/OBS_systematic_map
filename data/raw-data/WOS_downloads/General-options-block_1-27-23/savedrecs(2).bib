@article{ WOS:000309334000005,
Author = {Thiao, Djiga and Laloe, Francis},
Title = {A System of Indicators for Sustainability: An Example from the
   Senegalese Fisheries},
Journal = {MARINE RESOURCE ECONOMICS},
Year = {2012},
Volume = {27},
Number = {3},
Pages = {267-282},
Abstract = {In most coastal countries the fisheries sector satisfies multiple
   socioeconomic needs. This is especially the case in many coastal
   developing countries where fisheries represent a substantial source of
   food, jobs, and income. However, fisheries exert strong pressures on
   marine resources with threats to their sustainability. The context of
