@article{ WOS:000610325500001,
Author = {Anfuso, Giorgio and Postacchini, Matteo and Di Luccio, Diana and
   Benassai, Guido},
Title = {Coastal Sensitivity/Vulnerability Characterization and Adaptation
   Strategies: A Review},
Journal = {JOURNAL OF MARINE SCIENCE AND ENGINEERING},
Year = {2021},
Volume = {9},
Number = {1},
Month = {JAN},
Abstract = {Coastal area constitutes a vulnerable environment and requires special
   attention to preserve ecosystems and human activities therein. To this
   aim, many studies have been devoted both in past and recent years to
   analyzing the main factors affecting coastal vulnerability and
   susceptibility. Among the most used approaches, the Coastal
   Vulnerability Index (CVI) accounts for all relevant variables that
   characterize the coastal environment dealing with: (i) forcing actions
   (waves, tidal range, sea-level rise, etc.), (ii) morphological
   characteristics (geomorphology, foreshore slope, dune features, etc.),
   (iii) socio-economic, ecological and cultural aspects (tourism
   activities, natural habitats, etc.). Each variable is evaluated at each
   portion of the investigated coast, and associated with a vulnerability
   level which usually ranges from 1 (very low vulnerability), to 5 (very
   high vulnerability). Following a susceptibility/vulnerability analysis
   of a coastal stretch, specific strategies must be chosen and implemented
   to favor coastal resilience and adaptation, spanning from hard solutions
   (e.g., groins, breakwaters, etc.) to soft solutions (e.g., beach and
   dune nourishment projects), to the relocation option and the
   establishment of accommodation strategies (e.g., emergency
   preparedness).},
