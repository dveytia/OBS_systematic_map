@article{ WOS:000336035100027,
Author = {March, Hug and Sauri, David and Carles Llurdes, Joan},
Title = {Perception of the effects of climate change in winter and summer tourist
   areas: the Pyrenees and the Catalan and Balearic coasts, Spain},
