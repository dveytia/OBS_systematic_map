@article{ WOS:000614634100001,
Author = {McPhee, Daryl P. and Blount, Craig and Smith, Marcus P. Lincoln and
   Peddemors, Victor M.},
Title = {A comparison of alternative systems to catch and kill for mitigating
   unprovoked shark bite on bathers or surfers at ocean beaches},
Journal = {OCEAN \& COASTAL MANAGEMENT},
Year = {2021},
Volume = {201},
Month = {FEB 15},
Abstract = {Responses to unprovoked shark bite involve public policies and
   management approaches that contend with the needs of public safety and
   the responsibility to protect threatened species. In Australia
   (Queensland and New South Wales) and South Africa, methods that aim to
   capture and kill large sharks adjacent to popular beaches are a
   long-standing approach aimed at reducing the risk of shark bite. This
   paper reviews non-lethal alternatives to catch and kill methods, and
   suggests optimal conditions for non-lethal systems that will assist
   policy makers and beach authorities in choosing public safety responses
   that can be applied at the ocean beach scale. Deployment needs to be
   strategic with sufficient knowledge of their likely effectiveness under
   local conditions. At this stage we believe there is no single approach
   universally applicable to ocean beaches where unprovoked shark bite
   occurs, although well considered and locally appropriate mitigation
   measures can reduce risk.},
