@inproceedings{ WOS:000328117300080,
Author = {Tapete, D. and Bromhead, E. N.},
Editor = {Bilotta, E and Flora, A and Lirer, S and Viggiani, C},
Title = {Geohazards and climate change issues for the preservation of the Saxon
   Shore in South Britain},
Booktitle = {GEOTECHNICAL ENGINEERING FOR THE PRESERVATION OF MONUMENTS AND HISTORIC
   SITES},
Year = {2013},
Pages = {725-733},
Note = {2nd International Symposium on Geotechnical Engineering for the
   Preservation of Monuments and Historic Sites, Napoli, ITALY, MAY 30-31,
   2013},
Abstract = {The former environmental context and current visitors' perception of the
   Roman Shore Forts in South-East England belonging to the so-called
   ``Saxon Shore{''} (i.e. the Litus Saxonicum firstly mentioned in the
   Notitia Dignitatum, late 4th century AD) were drastically altered by
   centuries of sea-level rise and siltation phases due to past climate
   changes, and correlated coastal erosion and landsliding processes.
   Further threat for conservation is reasonably to be expected as a
   consequence of the near future climate change scenarios drawn for the
   UK. In this paper we analyze coastal and inland geohazards and
   consequent geotechnical instability which chronically affect the Roman
   Shore Forts, as well as the issues related to the implementation of
   protection schemes. The current land-locked configuration of sites like
   Richborough in Kent and Pevensey in East Sussex, makes them relatively
   or totally secure. Conversely, the direct and persistent exposure to
   coastal dynamics is a pressing issue for the conservation of the
   archaeological structures in Portchester, Hampshire and Reculver, North
   Kent. These sites clearly exemplify the current need of planning
   targeted conservation initiatives in the framework of an established and
   shared procedure of coastal risk management. They are assumed here among
   the demonstrative cases to critically discuss the impacts of climate
   change and ever-changing coastal environment on the choice of
   site-specific mitigation/adaptation strategies.},
Publisher = {CRC PRESS-TAYLOR \& FRANCIS GROUP},
Address = {6000 BROKEN SOUND PARKWAY NW, STE 300, BOCA RATON, FL 33487-2742 USA},
Type = {Proceedings Paper},
Language = {English},
