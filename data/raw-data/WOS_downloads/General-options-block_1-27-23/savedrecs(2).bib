   and Ibrahim, N. M. and Mohamad, N.},
Editor = {Sharif, S and Abdullah, MMAB and Rahim, SZA and Ghazali, MF and Saad, NM and Ramli, MM and Murad, SAZ and Isa, SSA},
Title = {Green Roof Technology-Mitigate Urban Heat Island (UHI) Effect},
