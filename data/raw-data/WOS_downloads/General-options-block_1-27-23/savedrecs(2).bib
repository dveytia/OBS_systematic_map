@inproceedings{ WOS:000265679400014,
Author = {Hitchcock, Christopher S. and Gailing, Richard W. and Lindvall, Scott C.},
Book-Group-Author = {ASME},
Title = {GEOTECHINCAL ASSESSMENT FOR MITIGATION OF A HIGH-PRESSURE PIPELINE
   ACROSS ACTIVE LANDSLIDES: DESIGN OF A DIRECTIONAL BORE IN SOUTHERN
   CALIFORNIA},
