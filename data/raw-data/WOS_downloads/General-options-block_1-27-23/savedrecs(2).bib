@article{ WOS:000171723000009,
Author = {El Raey, M and Dewidar, K and El Hattab, M},
Title = {Adaptation to the impacts of sea level rise in Egypt},
Journal = {CLIMATE RESEARCH},
Year = {1999},
Volume = {12},
Number = {2-3, SI},
Pages = {117-128},
Month = {AUG 27},
Abstract = {Assessment of the vulnerability of and expected socioeconomic losses
   over the Nile Delta coast due to the impact of sea level rise (SLR) was
   carried out in detail. Impacts of SLR on the Governorates of Alexandria
   and Port Said, in particular, were evaluated quantitatively. Options and
   costs of adaptation were analyzed and presented. Multi-criteria and
   decision matrix approaches based on questionnaire surveys were carried
   out to identify priorities in the 2 case studies. Results indicate that
   there are very limited possibilities of changing jobs for vulnerable
   stakeholders; cost is the main barrier of implementation; the majority
   of stakeholders recommend protection actions; and beach nourishment with
   limited hard structures (groins and breakwaters) is the best immediate
   option for adaptation, while the ICZM approach is the best available
   strategic option.},
