@article{ WOS:000361432000002,
Author = {Klijn, Frans and Kreibich, Heidi and de Moel, Hans and Penning-Rowsell,
   Edmund},
Title = {Adaptive flood risk management planning based on a comprehensive flood
   risk conceptualisation},
Journal = {MITIGATION AND ADAPTATION STRATEGIES FOR GLOBAL CHANGE},
