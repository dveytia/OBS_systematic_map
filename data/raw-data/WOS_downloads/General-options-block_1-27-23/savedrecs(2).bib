@article{ WOS:000840387600001,
Author = {Wei, Lili and Hong, Hualong and Bee, Ming Yang and Wu, Yangyang and
   Ndayambaje, Patrick and Yan, Chongling and Kao, Shuh-Ji and Chee, Poh
   Seng and Wang, Yuzhe},
Title = {Different adaptive strategies of three mangrove species to nutrient
   enrichment},
Journal = {PLANT ECOLOGY},
Year = {2022},
Volume = {223},
Number = {9},
Pages = {1093-1102},
Month = {SEP},
Abstract = {Mangrove species are undergoing environmental changes from nutrient-poor
   to enrichment due to the large nutrient input. The potential difference
   in adaptive strategies between the slow- and fast-growing species may
   lead to great changes in species interaction and ecosystem stability.
   This study aims to test whether the slow-growing species Aegiceras
   corniculatum (L.) Blanco and Kandelia obovata Sheue, Liu \& Yong sp.
   nov. are distinctly different from a fast-growing species Laguncularia
   racemosa (L.) Gaertn. f. in response to soil nutrient enrichment. With
   the increase of soil nutrients, L. racemosa shifted from a more
   conservative to a more acquisitive strategy. The potential causes
   included the increases in specific leaf area, nutrient resorption
   efficiency, and photosynthetic capacity as indicated by the increase of
   leaf delta C-13 and unchanged leaf succulence, as well as the relocation
   of photosynthetic products as indicated by the shift toward fast-growing
   at the cost of constructive and defense compounds. In contrast, A.
   corniculatum and K. obovata maintained conservative strategies at any
   soil nutrient levels with only a slight increase in growth. These
   findings implied that L. racemosa will be more competitive over the
   slow-growing species in nutrient-rich soils through altering adaptive
   strategies.},
