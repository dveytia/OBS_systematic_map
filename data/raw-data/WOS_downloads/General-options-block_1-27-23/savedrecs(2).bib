@article{ WOS:000441173100281,
Author = {Lam, Nina S. -N. and Qiang, Yi and Li, Kenan and Cai, Heng and Zou, Lei
   and Mihunov, Volodymyr},
Title = {Extending Resilience Assessment to Dynamic System Modeling: Perspectives
   on Human Dynamics and Climate Change Research},
Journal = {JOURNAL OF COASTAL RESEARCH},
Year = {2018},
Number = {85},
Pages = {1401-1405},
Month = {MAY},
Note = {15th International Coastal Symposium (ICS), Busan, SOUTH KOREA, MAY
   13-18, 2018},
Organization = {Korea Inst Ocean Sci \& Technol; Korea Soc Coastal Disaster Prevent;
   JCR; Coastal Educ \& Res Fdn},
Abstract = {It is widely known that the same type and strength of hazard could lead
   to very uneven impacts on different communities due to their varying
   vulnerability and resilience capacity. Hence, identifying the factors
   that make a community more resilient to hazards is critical to its
   sustainability and is central to climate change research and planning.
   This paper addresses three questions: what is the best way to measure
   community resilience to disasters and how to identify the key
   indicators? How do the resilience indicators dynamically interact in a
   quantitative manner that would lead to long-term resilience? And how can
   we translate the scientific results into practical tools for decision
   making? Using the population change pattern in the Mississippi River
   Delta as a case study, this paper demonstrates the use of a relatively
   new resilience assessment method called the Resilience Inference
   Measurement (RIM) method to measure resilience. Then, a newly developed
   spatial dynamic model is used to simulate population changes in the
   study area. The results show that without any changes in the current
   condition, the coastal portion of the study area will continue to suffer
   population loss and the region is unlikely to sustain in the future.},
