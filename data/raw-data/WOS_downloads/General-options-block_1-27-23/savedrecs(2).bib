@article{ WOS:000424641200004,
Author = {Kench, Paul S. and Ford, Murray R. and Owen, Susan D.},
Title = {Patterns of island change and persistence offer alternate adaptation
   pathways for atoll nations},
Journal = {NATURE COMMUNICATIONS},
Year = {2018},
Volume = {9},
Month = {FEB 9},
Abstract = {Sea-level rise and climatic change threaten the existence of atoll
   nations. Inundation and erosion are expected to render islands
   uninhabitable over the next century, forcing human migration. Here we
   present analysis of shoreline change in all 101 islands in the Pacific
   atoll nation of Tuvalu. Using remotely sensed data, change is analysed
   over the past four decades, a period when local sea level has risen at
   twice the global average (similar to 3.90 +/- 0.4 mm. yr(-1)). Results
   highlight a net increase in land area in Tuvalu of 73.5 ha (2.9\%),
   despite sea-level rise, and land area increase in eight of nine atolls.
   Island change has lacked uniformity with 74\% increasing and 27\%
   decreasing in size. Results challenge perceptions of island loss,
   showing islands are dynamic features that will persist as sites for
   habitation over the next century, presenting alternate opportunities for
   adaptation that embrace the heterogeneity of island types and their
