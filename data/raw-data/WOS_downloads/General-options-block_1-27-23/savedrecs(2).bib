Editor = {Lollino, G and Manconi, A and Guzzetti, F and Culshaw, M and Bobrowsky, P and Luino, F},
Title = {Geo-hydrological Risk Mitigation Measures and Land-Management in a
   Highly Vulnerable Small Coastal Catchment},
Booktitle = {ENGINEERING GEOLOGY FOR SOCIETY AND TERRITORY, VOL 5: URBAN GEOLOGY,
   SUSTAINABLE PLANNING AND LANDSCAPE EXPLOITATION},
Year = {2015},
