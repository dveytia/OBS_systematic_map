@inproceedings{ WOS:000309430200004,
Author = {Eldeberky, Y.},
Editor = {Benassai, G and Brebbia, CA and Rodriguez, G},
Title = {Coastal adaptation to sea level rise along the Nile delta, Egypt},
Booktitle = {COASTAL PROCESSES II},
