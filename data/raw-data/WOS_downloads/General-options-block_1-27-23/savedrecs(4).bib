@article{ WOS:000507687200001,
Author = {Harvey, Madeleine E. and Giddings, Sarah N. and Stein, Eric D. and
   Crooks, Jeffrey A. and Whitcraft, Christine and Gallien, Timu and
   Largier, John L. and Tiefenthaler, Liesl and Meltzer, Hallee and Pawlak,
   Geno and Thorne, Karen and Johnston, Karina and Ambrose, Richard and
   Schroeter, Stephen C. and Page, Henry M. and Elwany, Hany},
Title = {Effects of Elevated Sea Levels and Waves on Southern California
   Estuaries During the 2015-2016 El Nino},
Journal = {ESTUARIES AND COASTS},
Year = {2020},
Volume = {43},
Number = {2},
Pages = {256-271},
Month = {MAR},
Abstract = {The 2015-2016 El Nino provided insight into how low-inflow estuaries
   might respond to future climate regimes, including high sea levels and
   more intense waves. High waves and water levels coupled with low
   rainfall along the Southern California coastline provided the
   opportunity to examine how extreme ocean forcing impacts estuaries
   independently from fluvial events. From November 2015 to April 2016,
   water levels were measured in 13 Southern California estuaries,
   including both intermittently closed and perennially open estuaries with
   varying watershed size, urban development, and management practices.
   Elevated ocean water levels caused raised water levels and prolonged
   inundation in all of the estuaries studied. Water levels inside
   perennially open estuaries mirrored ocean water levels, while those
   inside intermittently closed estuaries (ICEs) exhibited enhanced
   higher-high water levels during large waves, and tides were truncated at
   low tides due to a wave-built sand sill at the mouth, resulting in
   elevated detided water levels. ICEs closed when sufficient wave-driven
   sand accretion formed a barrier berm across the mouth separating the
   estuary from the ocean, the height of which can be estimated using
   estuarine lower-low water levels. During the 2015-2016 El Nino, a
   greater number of Southern California ICEs closed than during a typical
   year and ICEs that close annually experienced longer than normal
   closures. Overall, sill accretion and wave exposure were important
   contributing factors to individual estuarine response to ocean
   conditions. Understanding how estuaries respond to increased sea levels
   and waves and the factors that influence closures will help managers
   develop appropriate adaptation strategies.},
