@article{ WOS:000630295600001,
Author = {Wedin, Anna},
Title = {Getting adaptation right - challenges and ethical issues facing planners
   adapting to sea level rise in southern Sweden},
Journal = {LOCAL ENVIRONMENT},
Year = {2021},
Volume = {26},
Number = {4},
Pages = {504-516},
Month = {APR 3},
Abstract = {Sea level rise (SLR) caused by climate change presents challenges for
   public planners all over the world. In this study, practical challenges
   and ethical factors in adaptation to SLR were identified based on
   interviews with planners in Sweden. Six challenges were identified: lack
   of knowledge and human resources, lacking integration and coordination,
   lacking or insufficient frameworks for dealing with long-time-horizons,
   responsibility for implementation and financing, and goal conflicts in
   the process of implementing adaptation policy. These challenges are then
   developed into a typology of ethical factors that are of particular
   importance for adaptation to SLR, categorised as input-oriented,
   process-oriented and outcome-oriented ethical factors. The challenges
   and ethical factors identified in this paper are the first step taken
   towards answering the question of what a sustainable and ethical
   adaptation to SLR in the context of local and regional policy-making
   could look like.},
