Author = {Archibald, R. K. and Evans, K. J. and Drake, J. B. and White, III, J. B.},
Editor = {Allen, G and Seidel, E and Dongarra, J and Nabrzyski, J and VanAlbada, GD and Sloot, PMA},
Title = {Time Acceleration Methods for Advection on the Cubed Sphere},
