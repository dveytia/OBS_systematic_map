@article{ WOS:000264346400007,
Author = {Symes, David and Akbar, Delwar and Gillen, Mike and Smith, Phil},
Title = {Land-use Mitigation Strategies for Storm Surge Risk in South East
   Queensland},
Journal = {AUSTRALIAN GEOGRAPHER},
Year = {2009},
Volume = {40},
Number = {1},
Pages = {121-136},
Abstract = {South East Queensland's (SEQ) potential exposure to tropical cyclone and
   storm surge events provides a significant dilemma for coastal
   settlements and their communities. It is essential for the long-term
   viability of SEQ's coastal settlements that effective land-use
   strategies are developed and implemented. This study develops a
