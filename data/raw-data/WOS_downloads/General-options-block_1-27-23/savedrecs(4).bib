@article{ WOS:000530231700002,
Author = {Taylor, Lotem and Curson, David and Verutes, Gregory M. and Wilsey, Chad},
Title = {Mapping sea level rise impacts to identify climate change adaptation
   opportunities in the Chesapeake and Delaware Bays, USA},
Journal = {WETLANDS ECOLOGY AND MANAGEMENT},
