@article{ WOS:000433190000037,
Author = {Sasaki, Yui and Matsuo, Kaoru and Yokoyama, Makoto and Sasaki, Masaru
   and Tanaka, Takahiro and Sadohara, Satoru},
Title = {Sea breeze effect mapping for mitigating summer urban warming: For
   making urban environmental climatemap of Yokohama and its surrounding
   area},
Journal = {URBAN CLIMATE},
Year = {2018},
Volume = {24},
Pages = {529-550},
Month = {JUN},
Abstract = {Temperatures in urban area rise; therefore, it is necessary to consider
   the mitigation of urban warming. Considering that, in coastal areas such
   as Yokohama, an effective mitigation could be based on sea breeze
   patterns during summer daytime, this study aims at analyzing the
   relationship between temperature distributions and sea breeze fronts.
   Eventually, the target of this paper could be the creation of an Urban
   Environmental Climate Map (UECM) showing mitigation measures for each
   zone. Firstly, fine weather days are extracted and the sea breeze
   blowing patterns are classified in the target area. Secondly, the wind
   and temperature distribution patterns are mapped using WRF. The results
   obtained enable the mapping of the sea breeze fronts and the analysis of
   the relationship between temperature distributions and sea breeze
   fronts. Finally, a spatial zoning is performed. The results are
   presented as follows: 1) Sea breeze blowing patterns are classified into
   three patterns; 2) Based on the numerical simulation results and the
   temperature data observed in coastal areas, southwest wind has a cooling
   effect; 3) The target area is classified into ``Sea breeze arrival
   zone{''} and others based on the presence of a see breeze; 4) An UECM
   showing proper mitigation measures is proposed. (C) 2017 Elsevier Inc.
   All rights reserved.},
