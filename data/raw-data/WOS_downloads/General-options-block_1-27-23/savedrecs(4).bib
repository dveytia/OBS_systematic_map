@article{ WOS:000409497100007,
Author = {Wadey, Matthew and Brown, Sally and Nicholls, Robert J. and Haigh, Ivan},
Title = {Coastal flooding in the Maldives: an assessment of historic events and
   their implications},
Journal = {NATURAL HAZARDS},
