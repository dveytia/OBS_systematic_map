Title = {Planting breadfruit orchards as a climate change adaptation strategy for
   the Pacific islands},
Booktitle = {XXIX INTERNATIONAL HORTICULTURAL CONGRESS ON HORTICULTURE: SUSTAINING
   LIVES, LIVELIHOODS AND LANDSCAPES (IHC2014): INTERNATIONAL SYMPOSIUM ON
   HORTICULTURE IN DEVELOPING COUNTRIES AND WORLD FOOD PRODUCTION},
Series = {Acta Horticulturae},
Year = {2016},
