@article{ WOS:000366834900003,
Author = {Moore, Roger and Davis, Geoff},
Title = {Cliff instability and erosion management in England and Wales},
Journal = {JOURNAL OF COASTAL CONSERVATION},
Year = {2015},
Volume = {19},
