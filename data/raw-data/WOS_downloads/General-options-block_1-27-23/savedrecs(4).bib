@article{ WOS:000502080800001,
Author = {Hofmann, M. and Mathesius, S. and Kriegler, E. and van Vuuren, D. P. and
   Schellnhuber, H. J.},
Title = {Strong time dependence of ocean acidification mitigation by atmospheric
   carbon dioxide removal},
Journal = {NATURE COMMUNICATIONS},
Year = {2019},
Volume = {10},
Month = {DEC 6},
Abstract = {In Paris in 2015, the global community agreed to limit global warming to
   well below 2 degrees C, aiming at even 1.5 degrees C. It is still
   uncertain whether these targets are sufficient to preserve marine
   ecosystems and prevent a severe alteration of marine biogeochemical
   cycles. Here, we show that stringent mitigation strategies consistent
   with the 1.5 degrees C scenario could, indeed, provoke a critical
   difference for the ocean's carbon cycle and calcium carbonate saturation
   states. Favorable conditions for calcifying organisms like tropical
   corals and polar pteropods, both of major importance for large
   ecosystems, can only be maintained if CO2 emissions fall rapidly between
   2025 and 2050, potentially requiring an early deployment of CO2 removal
   techniques in addition to drastic emissions reduction. Furthermore, this
   outcome can only be achieved if the terrestrial biosphere remains a
   carbon sink during the entire 21st century.},
