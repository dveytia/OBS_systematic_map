@article{ WOS:000299805400005,
Author = {Monzon, Javier and Moyer-Horner, Lucas and Palamar, Maria Baron},
Title = {Climate Change and Species Range Dynamics in Protected Areas},
Journal = {BIOSCIENCE},
Year = {2011},
Volume = {61},
Number = {10},
Pages = {752-761},
Month = {OCT},
Abstract = {Protected areas are key conservation tools for biodiversity management,
   but they are failing to protect species from current climate change.
   Focusing on protected areas representing montane, arid, coastal, and
   marine ecosystems, we provide examples of climate change induced range
   dynamics, includihg species' moving out of protected areas, disease
   range expansions, severe population declines, and even extinctions.
   Climate change thus presents an immense challenge to protected areas but
   also an unparalleled opportunity to shift from managing for static,
   historical community composition toward managing for dynamic, novel
   assemblages, thus complementing the traditional individual-species
   approach with an ecosystem-services approach: In addition, protected
   areas are well positioned to lead the way in climate change mitigation.
   Protected area managers can start achieving these goals by strengthening
   their commitments in climate change research, community outreach, and
   sustainability.},
