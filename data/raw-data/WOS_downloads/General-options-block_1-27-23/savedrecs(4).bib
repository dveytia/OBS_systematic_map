   Svenning, Jens-Christian},
Editor = {Basse, EM and Svenning, JC and Olesen, JE},
Title = {Impacts of 21(st) century climate changes on flora and vegetation in
   Denmark},
Booktitle = {BEYOND KYOTO: ADDRESSING THE CHALLENGES OF CLIMATE CHANGE - SCIENCE
   MEETS INDUSTRY, POLICY AND PUBLIC},
