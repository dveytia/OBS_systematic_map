@article{ WOS:000593276200001,
Author = {Boilard, Aurelie and Dube, Caroline E. and Gruet, Cecile and Merciere,
   Alexandre and Hernandez-Agreda, Alejandra and Derome, Nicolas},
Title = {Defining Coral Bleaching as a Microbial Dysbiosis within the Coral
   Holobiont},
Journal = {MICROORGANISMS},
Year = {2020},
Volume = {8},
Number = {11},
Month = {NOV},
Abstract = {Coral microbiomes are critical to holobiont health and functioning, but
   the stability of host-microbial interactions is fragile, easily shifting
   from eubiosis to dysbiosis. The heat-induced breakdown of the symbiosis
   between the host and its dinoflagellate algae (that is,
   ``bleaching{''}), is one of the most devastating outcomes for reef
   ecosystems. Yet, bleaching tolerance has been observed in some coral
   species. This review provides an overview of the holobiont's diversity,
   explores coral thermal tolerance in relation to their associated
   microorganisms, discusses the hypothesis of adaptive dysbiosis as a
   mechanism of environmental adaptation, mentions potential solutions to
   mitigate bleaching, and suggests new research avenues. More
   specifically, we define coral bleaching as the succession of three
   holobiont stages, where the microbiota can (i) maintain essential
   functions for holobiont homeostasis during stress and/or (ii) act as a
   buffer to mitigate bleaching by favoring the recruitment of thermally
   tolerant Symbiodiniaceae species (adaptive dysbiosis), and where (iii)
   environmental stressors exceed the buffering capacity of both microbial
   and dinoflagellate partners leading to coral death.},
