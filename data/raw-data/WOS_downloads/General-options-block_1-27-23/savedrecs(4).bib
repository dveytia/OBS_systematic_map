@article{ WOS:000748952500025,
Author = {Kumar, Pankaj and Avtar, Ram and Dasgupta, Rajarshi and Johnson, Brian
   Alan and Mukherjee, Abhijit and Ahsan, Md Nasif and Duc Cong Hiep Nguyen
   and Hong Quan Nguyen and Shaw, Rajib and Mishra, Binaya Kumar},
Title = {Socio-hydrology: A key approach for adaptation to water scarcity and
   achieving human well-being in large riverine islands},
Journal = {PROGRESS IN DISASTER SCIENCE},
Year = {2020},
Volume = {8},
Month = {DEC},
Abstract = {Rapid global changes (population growth, urbanization and frequent
   extreme weather conditions) have cumulatively affected local water
   bodies and resulted in unfavorable hydrological, ecological, and
   environmental changes in the major river systems. Particularly,
   communities in isolated riverine islands are heavily affected due to
   their poor adaptive capacities, which is well documented in the
   contemporary literature. The focal point for the vulnerability of these
   people lies in the water resources (drinking water availability,
   agricultural water quality, salt-water intrusion, flooding etc.) and the
   future interaction between human and water systems. This paper advocates
   the importance of socio-hydrological research in the context of
   enhancing social adaptive capacity as well as for developing a resilient
   water environment in three very large riverine islands in Asia:
   Fraserganj (India), Dakshin Bedkashi (Bangladesh) (both from the
   Ganges-Brahaputra-Meghna Delta) and Con Dao Island (Mekong River,
   Vietnam). It also explores how the nexus of human-water relations could
   be applied to improve adaptive measures to manage local water needs
   while mitigating undesirable changes to the hydrological cycle.
   Socio-hydrological models as an integrated tool can be used to quantify
   the feedbacks between water resources and society at multiple scales,
   with the aim of expediting stakeholder participation for sustainable
   water resource management. The proposed idea in this study will be
   helpful to sketch projections of alternatives that explicitly account
   for plausible and co-evolving trajectories of the socio-hydrological
   system, which will yield both insights into cause-effect relationships
   and help stakeholders to identify safe functioning space.},
Publisher = {ELSEVIER},
Address = {RADARWEG 29, 1043 NX AMSTERDAM, NETHERLANDS},
Type = {Article},
Language = {English},
Affiliation = {Kumar, P (Corresponding Author), Inst Global Environm Strategies, Nat Resources \& Ecosyst Serv, Hayama, Kanagawa, Japan.
   Kumar, Pankaj; Dasgupta, Rajarshi; Johnson, Brian Alan, Inst Global Environm Strategies, Nat Resources \& Ecosyst Serv, Hayama, Kanagawa, Japan.
   Avtar, Ram, Hokkaido Univ, Fac Environm Earth Sci, Sapporo, Hokkaido, Japan.
   Mukherjee, Abhijit, Indian Inst Technol, Sch Environm Sci \& Engn, Kharagpur 721302, W Bengal, India.
   Ahsan, Md Nasif, Khulna Univ, Econ Discipline, Khulna 9208, Bangladesh.
