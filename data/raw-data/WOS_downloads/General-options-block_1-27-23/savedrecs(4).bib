@article{ WOS:000488611200004,
Author = {Alcantara-Ayala, Irasema and Oliver-Smith, Anthony},
Title = {Early Warning Systems: Lost in Translation or Late by Definition? A
   FORIN Approach},
Journal = {INTERNATIONAL JOURNAL OF DISASTER RISK SCIENCE},
