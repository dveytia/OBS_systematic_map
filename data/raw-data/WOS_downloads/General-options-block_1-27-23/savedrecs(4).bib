@article{ WOS:000448999100016,
Author = {Zhang, Xiongqing and Lu, Lele and Cao, Quang V. and Duan, Aiguo and
   Zhang, Jianguo},
Title = {Climate-sensitive self-thinning trajectories of Chinese fir plantations
   in south China},
