Title = {Assessment of Aircraft Pilot Seat Performances with Digital Human Models
   and Virtual Prototypes},
Booktitle = {PROCEEDINGS OF THE 6TH INTERNATIONAL DIGITAL HUMAN MODELING SYMPOSIUM
   (DHM2020)},
Series = {Advances in Transdisciplinary Engineering},
Year = {2020},
Volume = {11},
