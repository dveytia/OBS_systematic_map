@inproceedings{ WOS:000184645800075,
Author = {Dewey, R and Stegen, G and Ozaki, M},
Editor = {Williams, DJ and Durie, B and McMullan, P and Paulson, C and Smith, A},
Title = {Ocean dispersion of CO2 from a moving ship source},
Booktitle = {GREENHOUSE GAS CONTROL TECHNOLOGIES},
Year = {2001},
Pages = {457-462},
Note = {5th International Conference on Greenshouse Gas Control Technologies,
   CAIRNS, AUSTRALIA, 2000},
Organization = {Int Energy Agcy, Australian Consortium; BHP; BP Amoco; CRIRO; Australian
   Greenhouse Off; Rio Tinto; Res Inst Innovat Technol Earth; Black Coal
   Utilisat Cooperat Res Ctr; Asea Brown Boveri; NOVEM},
Abstract = {Ocean disposal of CO2 may prove to be one of the mitigation strategies
   adopted to alleviate the build up of greenhouse gases in the atmosphere.
   At present, four primary ocean disposal scenarios are being evaluated
   through modelling and process studies. These include: 1) deep CO2 lake
   storage, 2) enhanced aqua-farming, 3) injection of CO2 from a fixed
   source, and 4) injection of CO2 from a moving ship source. Global ocean
   circulation models and carbon cycle models have previously simulated the
   spreading and sequestration efficiency of numerous fixed injection
   locations and depths (e.g. Dewey et al., 1996). In general, they have
   found that the injection of CO2 at greater depths and in quiescent
   regions of the ocean provides longer term buffering of the CO2 from the
   atmosphere. It has been suggested (Ozaki et al., 1999) that a moving
   injection source would enhance the initial dilution of the CO2 and
   reduce potential environmental impacts associated with high
   concentrations of CO2 near the injection point. We have adapted the
   Mesoscale Ocean Dispersion Model (MODM) to simulate the injection of CO2
   from a moving ship, and compare the resulting dispersion patterns with
   those simulated using a fixed source.},
