Author = {Cheung, William W. L. and Pinnegar, John and Merino, Gorka and Jones,
   Miranda C. and Barange, Manuel},
Title = {Review of climate change impacts on marine fisheries in the UK and
   Ireland},
Journal = {AQUATIC CONSERVATION-MARINE AND FRESHWATER ECOSYSTEMS},
