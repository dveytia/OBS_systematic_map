@inproceedings{ WOS:000185752400006,
Author = {Chaaban, FB and Kaysi, I and Chedid, R},
Editor = {Shen, S},
Title = {Contribution of transportation to GHG emissions - Case study of Lebanon},
Booktitle = {WORLD RESOURCE REVIEW, VOL 12, NOS 2-4: GLOBAL WARMING SCIENCE \&
   POLICY, PTS 1-3},
Series = {GLOBAL WARMING INTERNATIONAL CENTER MONOGRAPH SERIES},
Year = {2000},
Pages = {280-297},
Note = {11th International Conference on Global Warming Science and Policy,
   BOSTON, MA, APR 26-28, 2000},
Abstract = {Greenhouse gases (GHG), the main contributors to global warming are
   currently ranked amongst the most serious threats to the world's
   environment and social and economic development. GHG, and carbon dioxide
   in particular, are emitted from the transportation as well as other
   energy conversion sectors. This paper presents a comprehensive approach
   for conducting GHG emissions inventories from transportation in
   developing countries, and introducing the most appropriate mitigation
   options. The evaluation process is initiated by an inventory of GHG
   emissions in the base year. The inventory included all sectors of
   transportation: Land, sea, and air. Short- and long-term projections are
   then conducted taken into consideration the economic and social indices
   and the existing plans and regulations. Mitigation options applicable
   for Lebanon and for countries with similar social and economic
