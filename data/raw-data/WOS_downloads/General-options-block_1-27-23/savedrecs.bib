@article{ WOS:000295083900012,
Author = {Koehn, John D. and Hobday, Alistair J. and Pratchett, Morgan S. and
   Gillanders, Bronwyn M.},
Title = {Climate change and Australian marine and freshwater environments, fishes
   and fisheries: synthesis and options for adaptation},
