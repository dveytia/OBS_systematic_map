@article{ WOS:000180669000003,
Author = {Lokkeborg, S},
Title = {Review and evaluation of three mitigation measures - bird-scaring line,
   underwater setting and line shooter - to reduce seabird bycatch in the
   north Atlantic longline fishery},
Journal = {FISHERIES RESEARCH},
Year = {2003},
Volume = {60},
Number = {1},
Pages = {11-16},
Month = {JAN 30},
Abstract = {Seabirds scavenge baits from the hooks of commercial longlines,
   resulting in incidental seabird mortality and bait loss. As interactions
   between seabirds and longline fishing may cause seabird populations to
   decline and reduce gear efficiency, the potential of various mitigation
   measures to solve this problem has been tested. Four fishing experiments
   were conducted in commercial longlining in the north Atlantic to
   investigate the effectiveness of a bird-scaring line, underwater setting
   and a line shooter in reducing seabird bycatch during longline setting.
   These results are reviewed and the performance of the mitigation
   measures is evaluated. The great majority of the birds caught were
   northern fulmars (Fulmarus glacialis). Accidental catches of birds were
   reduced by all three methods, most clearly by the bird-scaring line. A
   total of 185 000 hooks were set with this mitigation measure in place,
   and only two birds were caught compared with 205 for control lines with
   a similar number of hooks set without any mitigation measure. The
   experiments also reduced bait loss and increased target species catch
   rates when mitigation measures were used. Increased catch rates are
   important incentives for fishermen to employ mitigation measures. It is
   concluded that the bird-scaring line is the most feasible and effective
   mitigation measure in the north Atlantic longline fishery, and the
   results reviewed should have relevance to longline fisheries worldwide
   in which interactions with Fulmarus spp. occur. (C) 2002 Elsevier
   Science B.V. rights reserved.},
