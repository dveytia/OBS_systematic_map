   Marina N. and Baschir, Laurentiu A.},
Editor = {Michel, U and Schulz, K and Ehlers, M and Nikolakopoulos, KG and Civco, DL},
Title = {Analysis of urbanization and climate change impacts on the urban thermal
   environment based on MODIS satellite data},
Booktitle = {EARTH RESOURCES AND ENVIRONMENTAL REMOTE SENSING/GIS APPLICATIONS V},
Series = {Proceedings of SPIE},
