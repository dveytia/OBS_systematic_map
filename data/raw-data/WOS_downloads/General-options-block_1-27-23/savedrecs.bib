@article{ WOS:000506918700006,
Author = {Cugnon, Gwenn and Caluwaerts, Steven and Duchene, Francois and Hamdi,
   Rafiq and Termonia, Piet and Top, Sara and Vergauwen, Thomas and Van
   Schaeybroeck, Bert},
Title = {Climate Sensitivity to Land Use Changes over the City of Brussels},
Journal = {GEOGRAPHICA PANNONICA},
Year = {2019},
Volume = {23},
Number = {4, SI},
Pages = {269-276},
Month = {DEC},
Abstract = {Prompted with the ongoing and projected climate change, a wide range of
   cities have committed, not only to mitigate greenhouse gas emissions but
   also to implement different climate change adaptation measures. These
   measures serve to ensure the wellbeing of the urban population. In
   practice, however, the planning of realistic adaptation measures is a
   complex process. Prior to starting such endeavor, it may therefore be
   useful to explore the maximum potential benefit that can be gained
   through adaptation measures. In this work, simple, extreme yet realistic
   adaptation measures are proposed in terms of changes in albedo and
   vegetation fraction. The impact of these land-use scenarios is explored
   by use of the land surface model SURFEX on the summer climate in terms
   of heat waves and the urban heat island for the city of Brussels. This
   is done for different periods in the future using the greenhouse gas
