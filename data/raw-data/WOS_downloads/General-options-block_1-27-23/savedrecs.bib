Title = {Thermal conditions during heat waves of a mid-European metropolis under
   consideration of climate change, urban development scenarios and
   resilience measures for the mid-21st century},
Journal = {METEOROLOGISCHE ZEITSCHRIFT},
Year = {2021},
Volume = {30},
