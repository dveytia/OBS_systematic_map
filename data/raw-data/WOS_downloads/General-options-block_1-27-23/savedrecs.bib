@article{ WOS:000285178700009,
Author = {Turtos Carbonell, Leonor and Meneses Ruiz, Elieza and Carrera Doral,
   Wenceslao and Zucchetti, Massimo},
Title = {GLOBAL AND LOCAL ATMOSPHERIC POLLUTION EVALUATION AND CONTROL.
   CHALLENGES FOR A SMALL ISLAND AND FOR DEVELOPING COUNTRIES},
