@article{ WOS:000427563600007,
Author = {Jones, Anthony C. and Hawcroft, Matthew K. and Haywood, James M. and
   Jones, Andy and Guo, Xiaoran and Moore, John C.},
Title = {Regional Climate Impacts of Stabilizing Global Warming at 1.5 K Using
   Solar Geoengineering},
