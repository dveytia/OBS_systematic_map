   and Robinson, T. and Lindahl, J. and Grace, D.},
Title = {Effects of climate change on the occurrence and distribution of
   livestock diseases},
Journal = {PREVENTIVE VETERINARY MEDICINE},
Year = {2017},
Volume = {137},
Number = {B, SI},
Pages = {119-129},
Month = {FEB 1},
