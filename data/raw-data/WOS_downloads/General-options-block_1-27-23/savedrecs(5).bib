@article{ WOS:000373685700003,
Author = {Ivkovic, Milos and Hamann, Andreas and Gapare, Washington J. and
   Jovanovic, Tom and Yanchuk, Alvin},
Title = {A framework for testing radiata pine under projected climate change in
   Australia and New Zealand},
