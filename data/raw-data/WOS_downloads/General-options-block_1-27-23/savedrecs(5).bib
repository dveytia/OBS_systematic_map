@article{ WOS:000071624900003,
Author = {Dahl, C},
Title = {Integrated coastal resources management and community participation in a
   small island setting},
Journal = {OCEAN \& COASTAL MANAGEMENT},
Year = {1997},
Volume = {36},
Number = {1-3},
Pages = {23-45},
Abstract = {The concept of integrated coastal zone management must be rethought to
   adapt it to the geography and social conditions of small islands.
   Because of their size, all small islands are in the coastal zone.
   Defining the whole of the island and its coastal wafers as the
   management area may weaken a program's effectiveness. An alternative may
   he to adapt special area management planning to the socio-spatial
   organization found on small islands. Social conditions on islands, where
   people live in relatively small cohesive communities, makes resource
   user participation in planning and management particularly germane. This
   paper outlines an argument for small islands to use participation as a
   central strategy of coastal management based on experience with natural
   resource management efforts on the island of Pohnpei in the Federated
   States of Micronesia. (C) 1997 Elsevier Science Ltd.},
