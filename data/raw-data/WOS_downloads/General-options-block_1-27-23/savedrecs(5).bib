@article{ WOS:000483698500033,
Author = {Alola, Andrew Adewale and Alola, Uju Violet and Akadiri, Seyi Saint},
Title = {Renewable energy consumption in Coastline Mediterranean Countries:
   impact of environmental degradation and housing policy},
Journal = {ENVIRONMENTAL SCIENCE AND POLLUTION RESEARCH},
