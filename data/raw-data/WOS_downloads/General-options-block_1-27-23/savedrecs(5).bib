@article{ WOS:000423387900011,
Author = {Khadiyanto, P. and Soetomo, S. and Hadi, S. P.},
Title = {Settlement adaptation on a seawater tide overflow area at the north part
   of Semarang, Indonesia},
Journal = {JOURNAL OF FLOOD RISK MANAGEMENT},
