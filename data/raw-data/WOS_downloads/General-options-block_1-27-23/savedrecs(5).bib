@article{ WOS:000439470600007,
Author = {Gustafson, Shelley and Cadena, Angela Joehl and Chinh Cong Ngo and
   Kawash, Ammar and Saenghkaew, Ienkate and Hartman, Paul},
Title = {Merging science into community adaptation planning processes: a
   cross-site comparison of four distinct areas of the Lower Mekong Basin},
Journal = {CLIMATIC CHANGE},
Year = {2018},
Volume = {149},
Number = {1, SI},
Pages = {91-106},
Month = {JUL},
Abstract = {Climate change is increasingly affecting rural areas worldwide. The
   Lower Mekong Basin (LMB) is at particular risk due to heat stress,
   changing rainfall patterns, rising sea levels, and more frequent and
   extreme climatic events. It is imperative that local-level adaptation
   plans are developed in a manner that builds resilience to these growing
   threats. Strategies for developing adaptation plans tend to comprise
   predominantly science-led or predominantly community-led processes. This
   study examines an approach that balances inputs from both processes in
   characterizing community vulnerability as a component of the adaptation
   planning workflow. Evaluation sites are located within four distinct
   sub-regions of the LMB: the Vietnam Mekong Delta, the Annamite Mountains
   of Lao PDR, the Cambodia central lowlands, and the mid-elevation forests
   of northern Thailand. Our results indicate that by merging science-based
   data with community-level perspective, knowledge gaps from both sides
   are filled and a more comprehensive understanding of vulnerability is
   factored into adaptation planning.},
