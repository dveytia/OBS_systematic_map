@article{ WOS:000613320400001,
Author = {Seddon, Nathalie and Smith, Alison and Smith, Pete and Key, Isabel and
   Chausson, Alexandre and Girardin, Cecile and House, Jo and Srivastava,
   Shilpi and Turner, Beth},
Title = {Getting the message right on nature-based solutions to climate change},
Journal = {GLOBAL CHANGE BIOLOGY},
Year = {2021},
Volume = {27},
