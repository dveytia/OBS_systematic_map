@inproceedings{ WOS:000833541600024,
Author = {Martinez, Adriana and Garcia, Katherine and Klauber, Cecilia and
   Overbye, Thomas J.},
Book-Group-Author = {IEEE},
Title = {Undergraduate Research on Design Considerations for a GMD Mitigation
   System},
Booktitle = {2020 IEEE KANSAS POWER AND ENERGY CONFERENCE (KPEC)},
Year = {2020},
Note = {IEEE Kansas Power and Energy Conference (KPEC), ELECTR NETWORK, JUL
   13-14, 2020},
Organization = {IEEE},
Abstract = {Geomagnetic disturbances (GMDs) are the result of coronal mass ejections
   (CMEs) from the sun. CMEs, such as solar flares, alter the magnetic and
   electric field of the earth. These changes induce DC voltage sources
   superimposed on transmission lines. The voltages result in the
   introduction of quasi-DC geomagnetically induced currents (GICs) to the
   AC synchronous power grid. These currents can cause half cycle
   saturation of transformers, harmonics, reactive power losses, and in
   extreme situations, widespread power outages. In this work, a
   planning-based GMD mitigation strategy is developed for large power
   systems. GIC blocking devices and system topology are leveraged in the
   design of strategies and systems to maintain preferable system
   operations despite a GMD. Simulation cases based on GIC blocking device
   installation and islanding plans will be used to create a strategy to
   minimize the effects of GMDs on the power grid.},
