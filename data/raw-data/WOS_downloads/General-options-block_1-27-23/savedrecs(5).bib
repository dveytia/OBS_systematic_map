@article{ WOS:000348003700013,
Author = {Gondim Guanais, Jose Hugo and Medeiros, Rodrigo Pereira and McConney,
   Patrick A.},
Title = {Designing a framework for addressing bycatch problems in Brazilian
   small-scale trawl fisheries},
Journal = {MARINE POLICY},
Year = {2015},
Volume = {51},
Pages = {111-118},
Month = {JAN},
Abstract = {This paper proposes a fisheries management approach to facilitate the
   adoption of new technologies for bycatch reduction (Bycatch Reduction
   Devices - BRD) in small-scale fisheries along Brazil's
   southern-southeastern coast. It is a flexible framework derived from the
   literature and from practical experience with small-scale fisheries and
   fisheries management in Brazil. The framework consists of four steps:
   (1) evaluating and understanding the fishing system, (2) conducting BRD
   experiments, (3) exploring scenarios for BRD adoption, and (4) providing
   fisheries management support. The principles and concepts of an
   ecosystem approach to fisheries (EAF), and of adaptive co-management
   (ACM) within social-ecological systems, guide the framework. (C) 2014
   Elsevier Ltd. All rights reserved.},
