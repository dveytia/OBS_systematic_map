@inproceedings{ WOS:000230438501074,
Author = {Hsu, JRC and Klein, AHF and Benedet, L},
Editor = {Smith, JM},
Title = {Geomorphic approach for mitigating beach erosion downdrift of littoral
   barriers},
Booktitle = {COASTAL ENGINEERING 2004, VOLS 1-4},
Year = {2005},
Pages = {2022-2034},
Note = {29th International Conference on Coastal Engineering, Lisbon, PORTUGAL,
   2004},
Organization = {APL; APS; APSS; Bosh, Rexroth Hydraudyne, BV; Etermar, SA; INAG; Irmaos
   Cavaco, SA; Somague Engenharia, SA},
Abstract = {Two of the most notable littoral barriers are impermeable inlet jetties
   at navigation channels and breakwaters for harbors. Due to their
   protruding lengths, these structures often obstruct the natural flow of
   sediment alongshore, thus causing downdrift beach erosion. This study
   uses the parabolic bay shape equation derived for bay beaches in static
   equilibrium to examine the geomorphic effect of jetties and breakwaters
   on downdrift beaches, in which many were originally straight, but now in
   curved planform. Software MEPBAY is employed to visually assess the
   optimum positioning for the tips of these structures which are vital for
   the stability of downdrift beach. This geomorphic approach can be
   applied readily to strategically locate the tip of a harbor breakwater
   or inlet jetty in order to mitigate potential beach erosion. The same
   approach is also applicable to any bayed beaches for environmental
   impact assessment and coastal management, for design of a recreational
   beach and shore protection.},
