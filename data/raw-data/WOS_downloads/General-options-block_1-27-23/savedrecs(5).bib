@inproceedings{ WOS:000392270100086,
Author = {van Ruiten, Kees and Bles, Thomas and Kiel, Jan},
Editor = {Lang, M and Klijn, F and Samuels, P},
Title = {EU-INTACT-case studies: Impact of extreme weather on critical
   Infrastructure},
Booktitle = {3RD EUROPEAN CONFERENCE ON FLOOD RISK MANAGEMENT (FLOODRISK 2016)},
Series = {E3S Web of Conferences},
