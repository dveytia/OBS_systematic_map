@inproceedings{ WOS:000393731500008,
Author = {Liu, Qiusheng and Fukuda, Katsuya and Zhu, Daoxing},
Editor = {Zhang, L},
Title = {Solution and Diffusion Process of Carbon Dioxide in Seawater},
Booktitle = {PROCEEDINGS OF ISHTEC2012, 4TH INTERNATIONAL SYMPOSIUM ON HEAT TRANSFER
   AND ENERGY CONSERVATION},
