@article{ WOS:000269384200018,
Author = {De Luis, Martin and Novak, Klemen and Cufar, Katarina and Raventos, Jose},
Title = {Size mediated climate-growth relationships in Pinus halepensis and Pinus
   pinea},
Journal = {TREES-STRUCTURE AND FUNCTION},
Year = {2009},
Volume = {23},
Number = {5},
Pages = {1065-1073},
Month = {OCT},
Abstract = {Functional processes in trees undergo changes as the tree size
   increases, which may affect the response of trees to environmental
   factors. We tested tree-ring response to climate in four groups of
   trees, including large and small Pinus halepensis and Pinus pinea trees
   using cluster, principal component (PC) and dendroclimatological
   analysis. The trees were of the same age and growing on a plantation in
   the semiarid coastal area of southern Spain. Cluster and PC analyses
   showed a clear separation into four groups of trees. Autocorrelation and
   mean sensitivity showed significant differences between the two size
   classes. PCA recognised four representative principal components where
   PC1 represented the tree-ring-climate variability common to both species
   and sizes, whereas PC2, PC3 and PC4 represented a species-specific and
   size-dependent response of trees to climate. The differences between the
   two size classes were greater than those between the two species. The
   results suggest that future tree-ring studies should include trees
   stratified by size. Only this would make it possible to produce unbiased
   predictions on the consequences of climate change and to devise suitable
   mitigation strategies for preserving Mediterranean forest ecosystems.},
