@article{ WOS:000457690600100,
Author = {Duarte, Carlos M. and Wu, Jiaping and Xiao, Xi and Bruhn, Annette and
   Krause-Jensen, Dorte},
Title = {Can Seaweed Farming Play a Role in Climate Change Mitigation and
   Adaptation?},
Journal = {FRONTIERS IN MARINE SCIENCE},
Year = {2017},
Volume = {4},
Abstract = {Seaweed aquaculture, the fastest-growing component of global food
   production, offers a slate of opportunities to mitigate, and adapt to
   climate change. Seaweed farms release carbon that maybe buried in
   sediments or exported to the deep sea, therefore acting as a CO2 sink.
   The crop can also be used, in total or in part, for biofuel production,
   with a potential CO2 mitigation capacity, in terms of avoided emissions
   from fossil fuels, of about 1,500 tons CO2 km(-2) year(-1). Seaweed
   aquaculture can also help reduce the emissions from agriculture, by
   improving soil quality substituting synthetic fertilizer and when
   included in cattle fed, lowering methane emissions from cattle. Seaweed
   aquaculture contributes to climate change adaptation by damping wave
   energy and protecting shorelines, and by elevating pH and supplying
   oxygen to the waters, thereby locally reducing the effects of ocean
   acidification and de-oxygenation. The scope to expand seaweed
   aquaculture is, however, limited by the availability of suitable areas
   and competition for suitable areas with other uses, engineering systems
   capable of coping with rough conditions offshore, and increasing market
   demand for seaweed products, among other factors. Despite these
   limitations, seaweed farming practices can be optimized to maximize
   climate benefits, which may, if economically compensated, improve the
   income of seaweed farmers.},
