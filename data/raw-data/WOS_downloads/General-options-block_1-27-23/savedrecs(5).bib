@article{ WOS:000546956600031,
Author = {da Silva-Pinto, Thalles and Silveira, Mayara Moura and de Souza, Jessica
   Ferreira and Moreira, Ana Luisa Pires and Vieira, Edson Aparecido and
   Longo, Guilherme Ortigara and Luchiari, Ana Carolina},
Title = {Damselfish face climate change: Impact of temperature and habitat
   structure on agonistic behavior},
Journal = {PLOS ONE},
Year = {2020},
Volume = {15},
Number = {6},
Month = {JUN 30},
Abstract = {Oceans absorb a huge part of the atmospheric heat, leading to the rise
   in water temperature. Reefs are among the most affected ecosystems,
   where the complex behavioral repertoire of fishes is usually an
   indicator of environmental impacts. Here, we examined whether
   temperature (28 and 34 degrees C) and habitat complexity (high and low)
   interact to affect the agonistic behavior (mirror test) of the dusky
   damselfish (Stegastes fuscus), a key species in Brazilian reefs because
   of its gardening capacity and territorial behavior. Higher temperatures
   altered basal behavior in both high and low-complexity conditions. Fish
   kept at 28 degrees C under the high-complexity condition were more
   aggressive than those at a higher temperature (34 degrees C) and in a
   low-complexity condition, which also exhibited lower dispersion. Our
   data show that changes in behavior of coral reef fish is associated to
   fluctuations in environmental conditions. Thus, it is important to
   implement management or conservation strategies that could mitigate
   global change effects.},
Publisher = {PUBLIC LIBRARY SCIENCE},
