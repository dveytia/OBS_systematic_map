Address = {530 WALNUT STREET, STE 850, PHILADELPHIA, PA 19106 USA},
Type = {Article},
Language = {English},
Affiliation = {Bilkovic, DM (Corresponding Author), Virginia Inst Marine Sci, Coll William \& Mary, POB 1346, Gloucester Point, VA 23062 USA.
   Bilkovic, Donna Marie; Mitchell, Molly; Mason, Pam; Duhring, Karen, Virginia Inst Marine Sci, Coll William \& Mary, POB 1346, Gloucester Point, VA 23062 USA.},
DOI = {10.1080/08920753.2016.1160201},
ISSN = {0892-0753},
EISSN = {1521-0421},
Keywords = {climate adaptation; coastal management; estuarine shoreline protection;
   marsh; wetland restoration},
Keywords-Plus = {FRINGING SALT MARSHES; SEA-LEVEL RISE; ADAPTIVE MANAGEMENT; COASTAL
