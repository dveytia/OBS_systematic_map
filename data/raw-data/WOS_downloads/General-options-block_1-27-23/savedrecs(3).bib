@article{ WOS:000666451800001,
Author = {Timilsina, Govinda R.},
Title = {Financing Climate Change Adaptation: International Initiatives},
Journal = {SUSTAINABILITY},
Year = {2021},
Volume = {13},
Number = {12},
Month = {JUN},
Abstract = {Climate change adaptation is one of the main strategies to address
   global climate change. The least developed countries and the small
   island states that lack financial resources to adapt to climate change
   are the most vulnerable nations to climate change. Although it would be
   more economical to adapt to climate change compared to the anticipated
   damage of not doing so, the demand for capital is estimated to range to
   hundreds of billions. The crucial question is how to manage investments
   to adapt to climate change globally. This study provides an overview of
   existing international provisions on climate finance for adaptation. It
   includes provisions through international financial institutions, United
   Nations agencies, bilateral and multilateral channels, and the private
   sector. It also explores how private sector finance can be further
   attracted to invest in climate change adaptation.},
