@inproceedings{ WOS:000320967700120,
Author = {Kashani, Reza and Monfort, Jeff},
Book-Group-Author = {ASME},
Title = {LOW-FREQUENCY THERMOACOUSTIC INSTABILITY MITIGATION USING
   ADAPTIVE-PASSIVE ACOUSTIC RADIATORS},
Booktitle = {PROCEEDINGS OF THE ASME TURBO EXPO 2011, VOL 2, PTS A AND B},
