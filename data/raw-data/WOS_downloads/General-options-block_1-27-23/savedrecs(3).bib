@article{ WOS:000371485300010,
Author = {Bombar, Gokcen and Dolgen, Deniz and Alpaslan, M. Necdet},
Title = {Environmental impacts and impact mitigation plans for desalination
   facilities},
Journal = {DESALINATION AND WATER TREATMENT},
