@article{ WOS:000540253100007,
Author = {Weiskopf, Sarah R. and Rubenstein, Madeleine A. and Crozier, Lisa G. and
   Gaichas, Sarah and Griffis, Roger and Halofsky, Jessica E. and Hyde,
   Kimberly J. W. and Morelli, Toni Lyn and Morisette, Jeffrey T. and
