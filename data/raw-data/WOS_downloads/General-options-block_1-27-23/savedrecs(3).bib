@article{ WOS:000355349600009,
Author = {Angeon, Valerie and Bates, Samuel},
Title = {Reviewing Composite Vulnerability and Resilience Indexes: A Sustainable
   Approach and Application},
Journal = {WORLD DEVELOPMENT},
Year = {2015},
Volume = {72},
Pages = {140-162},
Month = {AUG},
Abstract = {Vulnerability resilience indexes fail to grasp all dimensions of
   sustainability, whereas sustainable development has gained momentum. We
   fill this gap with a hierarchical multimetric composite index
   (Net-Vulnerability Resilience Index: NVRI) whose robustness relies on a
   mathematical algorithm based on graph theory. A worldwide application
   shows that (i) both vulnerability and resilience are policy-responsive
   and that (ii) there is no determinism for a country to remain vulnerable
   or resilient. The NVRI enables us to identify a country's strengths and
   weaknesses and determine the policy orientations that should be
   implemented to achieve sustainability. (C) 2015 Elsevier Ltd. All rights
   reserved.},
