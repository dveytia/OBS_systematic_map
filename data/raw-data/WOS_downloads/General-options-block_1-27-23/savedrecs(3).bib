@article{ WOS:000239752200004,
Author = {Folke, Carl},
Title = {Resilience: The emergence of a perspective for social-ecological systems
   analyses},
Journal = {GLOBAL ENVIRONMENTAL CHANGE-HUMAN AND POLICY DIMENSIONS},
Year = {2006},
