@article{ WOS:000461111800007,
Author = {Cherchi, Annalisa and Ambrizzi, Tercio and Behera, Swadhin and Vasques
   Freitas, Ana Carolina and Morioka, Yushi and Zhou, Tianjun},
Title = {The Response of Subtropical Highs to Climate Change},
