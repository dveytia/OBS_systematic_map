Article-Number = {3391},
EISSN = {2073-4441},
Keywords = {governance; water; climate; deltas; transformation; resilience},
Keywords-Plus = {SEA-LEVEL RISE; ADAPTIVE POLICY PATHWAYS; ENVIRONMENTAL GOVERNANCE;
