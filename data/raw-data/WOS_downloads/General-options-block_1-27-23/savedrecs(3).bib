@article{ WOS:000169966700036,
Author = {Wigley, TML and Raper, SCB},
Title = {Interpretation of high projections for global-mean warming},
Journal = {SCIENCE},
Year = {2001},
Volume = {293},
Number = {5529},
Pages = {451-454},
Month = {JUL 20},
Abstract = {The Intergovernmental Panel on Climate Change (IPCC) has recently
   released its Third Assessment Report (TAR), in which new projections are
   given for global-mean warming in the absence of policies to limit
   climate change. The full warming range over 1990 to 2100, 1.4 degrees to
   5.8 degreesC, is substantially higher than the range given previously in
   the IPCC Second Assessment Report. Here we interpret the new warming
   range in probabilistic terms, accounting for uncertainties in emissions,
   the climate sensitivity, the carbon cycle, ocean mixing, and aerosol
   forcing. We show that the probabilities of warming values at both the
   high and Low ends of the TAR range are very Low. In the absence of
   climate-mitigation policies, the 90\% probability interval for 1990 to
   2100 warming is 1.7 degrees to 4.9 degreesC.},
