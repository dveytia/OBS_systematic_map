@article{ WOS:000378931800017,
Author = {Sahoo, Amiya Kumar and Sharma, Anil Prakash and Suresh, V. R.},
Title = {Managing Rivers: Ecohydrology an Effective Tool Under Changing Climate},
Journal = {NATIONAL ACADEMY SCIENCE LETTERS-INDIA},
Year = {2016},
Volume = {39},
Number = {3},
Pages = {229-232},
Month = {JUN},
Abstract = {Rivers, their associated floodplains and wetlands are considered as the
   lifeline of millions. Globally these ecosystems are now considered as
   the most degraded, and losing species at a rate faster than the
   terrestrial and marine systems. Serious anthropogenic pressures and
   climate change are likely to intensify the degradation processes
   further. Hence, need of the hour is to develop a holistic management
   tool for protecting these freshwater ecosystems. Ecohydrology is a tool
   that integrates hydrology, geomorphology and biodiversity and forms a
   framework at catchment level for protecting the physical modification
   and biodiversity in rivers with the basic concept of environmental
   flows. Though the concept of environmental flows/ecological release for
   Indian rivers is recently introduced in national water policy, it
   further needs to be suitably incorporated in the climate adaptation
   strategies for an effective integrated water resources development and
   management programme.},
