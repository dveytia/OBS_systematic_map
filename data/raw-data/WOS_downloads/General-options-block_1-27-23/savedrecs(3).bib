@article{ WOS:000288899500010,
Author = {Kavun, V. Ya. and Chepkasova, A. I. and Podgurskaya, O. V. and Kovalev,
   N. N.},
Title = {Adaptation of the cholinergic system of Crenomytilus grayanus (Bivalvia,
   Mytilidae) to the impact of natural and anthropogenic conditions},
Journal = {BIOLOGY BULLETIN},
Year = {2011},
Volume = {38},
Number = {2},
Pages = {176-182},
Month = {APR},
Abstract = {The effects of environmental conditions on cholinesterase activity and
   kinetic parameters of substrate hydrolysis in the hemolymph of the
   mussel Crenomytilus grayanus were studied. Under seasonal upwellings,
   the cholinergic system efficiency is provided for by a wide range of
   efficient concentrations of the substrate, i.e., under such conditions
   the mussels at the molecular level have a quantitative adaptation
   strategy of the enzyme. In mussels from the stationary upwelling zone
   (at a steady low temperature of water) for efficiency of the cholinergic
   system, the quantitative strategy of enzyme adaptation is realized. In
   mussels from a highly contaminated site, irreversible damages to the
   cholinergic process were observed. The affinity of the substrate to the
   enzyme is highly informative and an appropriate biomarker for the load
   level and the adaptation capacity of the organism. The affinity of the
   substrate to the enzyme is recommended as a new biomarker.},
