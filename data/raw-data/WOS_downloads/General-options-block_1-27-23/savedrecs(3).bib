@article{ WOS:000453497100004,
Author = {Bundhoo, Zumar M. A. and Shah, Kalim U. and Surroop, Dinesh},
Title = {Climate proofing island energy infrastructure systems: Framing
   resilience based policy interventions},
Journal = {UTILITIES POLICY},
Year = {2018},
Volume = {55},
Pages = {41-51},
Month = {DEC},
Abstract = {One of the most discernible impacts of climate change is the increasing
   severity and unpredictability of extreme weather conditions. Small
   island developing states are particularly vulnerable to these
   conditions, with one of the impacts being on energy supplies due to
   damaging energy infrastructure, resulting in power outages and economic
   losses. Drawing on resilience theory to frame the discourse, we present
   an updated overview of how island energy infrastructures have been and
   continue to be negatively impacted. This same framework also provides a
   lens through which we identify the challenges involved in recovery,
   rebuilding and returning energy security in these contexts.},
