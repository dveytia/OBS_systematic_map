@article{ WOS:000502229600001,
Author = {English, Elizabeth C. and Chen, Meiyi and Zarins, Rebecca and Patange,
   Poorna and Wiser, Jeana C.},
Title = {Building Resilience through Flood Risk Reduction: The Benefits of
   Amphibious Foundation Retrofits to Heritage Structures},
