@article{ WOS:000450062200001,
Author = {Sharma, A. and Hamlet, A. F. and Fernando, H. J. S. and Catlett, C. E.
   and Horton, D. E. and Kotamarthi, V. R. and Kristovich, D. A. R. and
   Packman, A. I. and Tank, J. L. and Wuebbles, D. J.},
Title = {The Need for an Integrated Land-Lake-Atmosphere Modeling System,
