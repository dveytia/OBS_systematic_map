@article{ WOS:000365177300026,
Author = {Nay, Tiffany J. and Johansen, Jacob L. and Habary, Adam and Steffensen,
   John F. and Rummer, Jodie L.},
Title = {Behavioural thermoregulation in a temperature-sensitive coral reef fish,
   the five-lined cardinalfish (Cheilodipterus quinquelineatus)},
Journal = {CORAL REEFS},
Year = {2015},
Volume = {34},
Number = {4},
Pages = {1261-1265},
Month = {DEC},
Abstract = {As global temperatures increase, fish populations at low latitudes are
   thought to be at risk as they are adapted to narrow temperature ranges
   and live at temperatures close to their thermal tolerance limits.
   Behavioural movements, based on a preference for a specific temperature
   (T (pref)), may provide a strategy to cope with changing conditions. A
   temperature-sensitive coral reef cardinalfish (Cheilodipterus
   quinquelineatus) was exposed to 28 A degrees C (average at collection
   site) or 32 A degrees C (predicted end-of-century) for 6 weeks. T (pref)
   was determined using a shuttlebox system, which allowed fish to
   behaviourally manipulate their thermal environment. Regardless of
   treatment temperature, fish preferred 29.5 +/- A 0.25 A degrees C,
   approximating summer average temperatures in the wild. However, 32 A
   degrees C fish moved more frequently to correct their thermal
   environment than 28 A degrees C fish, and daytime movements were more
   frequent than night-time movements. Understanding temperature-mediated
   movements is imperative for predicting how ocean warming will influence
   coral reef species and distribution patterns.},
