@article{ WOS:000426591900015,
Author = {Emmanuel, Rohinton},
Title = {Performance standard for tropical outdoors: A critique of current
   impasse and a proposal for way forward},
Journal = {URBAN CLIMATE},
Year = {2018},
Volume = {23},
Number = {SI},
Pages = {250-259},
Month = {MAR},
Abstract = {Climate change will bring more heat extremes to the already warm
   tropical areas in Asia, Africa and Latin America. The resulting surge in
   cooling demand will only make the thermal discomfort in tropical cities
   even worse. This paper presents evidence to the approaching air
   conditioned cooling load deluge and shows why the air conditioned
   management of cooling load is particularly problematic in the tropics.
   It then presents the technical, institutional and cultural challenges to
   passive climate-sensitive design in the tropics and posits three ideas
   as part of the way forward: reformulation of thermal pleasure in the
   tropics, linking the `in' and the `out' at neighbourhood scale and
   provision of adaptive opportunities varied by activity patterns.
   Research approaches to advance these ideas are also discussed. (c) 2017
   Elsevier B.V. All rights reserved.},
