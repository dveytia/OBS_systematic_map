@article{ WOS:000606846900001,
Author = {Hoque, Muhammad Al-Amin and Pradhan, Biswajeet and Ahmed, Naser and
   Ahmed, Bayes and Alamri, Abdullah M.},
Title = {Cyclone vulnerability assessment of the western coast of Bangladesh},
