@article{ WOS:000696997500005,
Author = {Taylor, Matthew D. and Hale, Darren and Johnson, Daniel D.},
Title = {Biological evaluation of an existing spatial management network for an
   exploited penaeid species},
Journal = {REGIONAL STUDIES IN MARINE SCIENCE},
Year = {2021},
Volume = {47},
Month = {SEP},
Abstract = {Spatial management may be employed to achieve objectives aimed at
   maintaining or enhancing productivity and sustainability of exploited
   populations. Spatial management is used in penaeid fisheries to alter
   the spatiotemporal distribution of fishing effort to achieve outcomes
   such as avoidance of undesirable size-classes. Eastern King Prawn
   (Penaeus {[}melicertus] plebejus; EKP) is a valuable penaeid species for
   which harvest is supported by an extensive spatial management network,
   however these closures have never been evaluated. We examine temporal
   variation in biomass and size structures within three trawl closure
   areas spanning four degrees of latitude. There was substantial spatial
   variation in EKP biomass among locations, fished and non-fished zones
   and sampling periods, which appeared to be influenced by depth,
   temperature and distance to the adjacent estuarine nursery. Size
   structure varied among locations and sampling periods, and differed
   inside and outside of closure areas, however in many cases the majority
   of prawns that were protected within closure areas were consistently
   smaller than the estimated size at first capture for optimal yield per
   recruit. The data suggest that the closures are likely to be effective
   at achieving their objective, but there may be some scope for adaptive
   management. Crown Copyright (C) 2021 Published by Elsevier B.V. All
   rights reserved.},
