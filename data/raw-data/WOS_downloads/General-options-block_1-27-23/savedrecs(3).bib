@article{ WOS:000411875800014,
Author = {Hooyberghs, Hans and Verbeke, Stijn and Lauwaet, Dirk and Costa, Helia
   and Floater, Graham and De Ridder, Koen},
Title = {Influence of climate change on summer cooling costs and heat stress in
   urban office buildings},
Journal = {CLIMATIC CHANGE},
