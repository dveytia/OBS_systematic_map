@article{ WOS:000317456100025,
Author = {Ahmed, Nesar and Occhipinti-Ambrogi, Anna and Muir, James F.},
Title = {The impact of climate change on prawn postlarvae fishing in coastal
   Bangladesh: Socioeconomic and ecological perspectives},
Journal = {MARINE POLICY},
Year = {2013},
Volume = {39},
Pages = {224-233},
Month = {MAY},
Abstract = {In Bangladesh, prawn (Macrobrachium rosenbergii) farming remains
   dependent on the capture of wild postlarvae as hatchery production is
   still inadequate. However, prawn postlarvae fishing has been accompanied
   by concerns over recent climate change. Different climatic variables
   including cyclone, salinity, sea level rise, water temperature, flood,
   rainfall, and drought have had adverse effects on coastal ecosystem,
   thus determining a decline in the availability of prawn postlarvae and
   thereby catch. The households of postlarvae fishers also face a variety
   of socioeconomic constraints due to climate change. Considering extreme
   vulnerability to the effects of climate change, an integrated approach
   needs to be introduced to cope with the challenges. (c) 2012 Elsevier
   Ltd. All rights reserved.},
