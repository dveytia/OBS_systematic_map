@inproceedings{ WOS:000493896300034,
Author = {Oliveira, Susana and Costa Pinto, Ligia M. and Costa, Ana},
Editor = {Ferreira, P and Soares, I},
Title = {CHOICE EXPERIMENTS TO ELICIT THE USERS' PREFERENCES FOR COASTAL EROSION
   MANAGEMENT - THE CASE OF PRAIA DA AMOROSA},
Booktitle = {PROCEEDINGS OF THE 4TH INTERNATIONAL CONFERENCE ON ENERGY \& ENVIRONMENT
   (ICEE 2019): BRINGING TOGETHER ENGINEERING AND ECONOMICS},
Series = {ICEE International Conference on Energy \& Environment},
Year = {2019},
Pages = {243-248},
Note = {4th International Conference on Energy and Environment - Bringing
   together Engineering and Economics (ICEE), Guimaraes, PORTUGAL, MAY
   16-17, 2019},
Organization = {Univ Minho, Sch Engn; Univ Porto, Sch Econ \& Management; UP, Ctr Econ
   \& Finance; CentroAlgoritmi},
Abstract = {Coastal erosion is a complex problem, mainly in terms of its effects and
   management strategies. Based on the increasing coastal erosion problem,
   it is crucial to ensure shore protection, considering all stakeholders'
   perspectives. This research investigates users' preferences for various
   options to deal with costal erosion, through a discrete choice
   experiment, and concludes that the respondents prefer some interventions
   to mitigate the problem than no action, and prefer lighter intervention
   (palisades, gangways) to heavy infrastructures (rockfills, seawalls,
   groynes). Moreover, the results show the presence of preference
   heterogeneity and thus the need to use more flexible and complex models.
   Based on the results obtained it is possible to drive some policy
   implications. First, the do-nothing option is not viable from the
   population standpoint; second, although some time of coastal erosion
   protection is demanded by the general population, the preferred approach
   is for light forms, contrary to the policy adopted in the last century,
   and that which still characterizes most existent coastal defense
   infrastructures. Lastly, there is considerable heterogeneity in
   respondents' preferences, demanding careful consideration of the welfare
   impact of coastal interventions on population segments.},
